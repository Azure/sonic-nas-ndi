module sonic-fdb-annot {

    yang-version "";

    namespace "http://openconfig.net/yang/annotation";
    prefix "sfdb-annot";

    import ietf-yang-types { prefix yang; }
    import sonic-fdb { prefix sfdb; }

    deviation /sfdb:sonic-fdb/sfdb:FDB_TABLE/sfdb:FDB_TABLE_LIST {
      deviate add {
            sonic-ext:db-name "APPL_DB";
      }
    }
}

