<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY NAME_SPACE                     "http://www.dellemc.com/networking/os10/dell-dns">
<!ENTITY SUBTREE_FILTER                 "Filter_Type:SUBTREE">
<!ENTITY NAMESPACE_FILTER              "Filter_Namespace:http://www.dellemc.com/networking/os10/dell-dns">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >

    <VIEW
        name="enable-view"
        >
         <COMMAND
             name="show hosts"
             help="show information about DNS"
             operation="get"
             namespace="&NAME_SPACE;"
             template="show_hosts.xsl"
             yang_filter="&SUBTREE_FILTER;;Filter_Conditions:/dns-resolver;&NAMESPACE_FILTER;"
             >
            <ACTION
                builtin="clish_extn"
                />
         </COMMAND>
     </VIEW>

    <!--=================================================================-->
    <!-- ********************************************************************** -->
    <!-- *  CONFIGURATION COMMANDS                                            * -->
    <!-- *  View : configure-view                                             * -->
    <!-- ********************************************************************** -->

    <VIEW
        name="configure-view"
        >

        <COMMAND
            name="ip domain-name"
            help="Define default domain name"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            >
            <PARAM
                name="name"
                help="Define the default domain name"
                ptype="STRING"
                yang_name="/dns-resolver/hostname=${name}"
                >
            </PARAM>	
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>

        <COMMAND
            name="no ip domain-name"
            help="Delete default domain name"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            yang_name="/dns-resolver/hostname"
            >
            <PARAM
                name="name"
                help="default domain name"
                ptype="STRING"
                optional="true"
                yang_name="/dns-resolver/hostname"
                >
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>

         <COMMAND
             name="ip domain-list"
             help="Domain name to complete unqualified host name; Domain names limited to six domains with a total of 256 characters"
             operation="edit-config"
             namespace="&NAME_SPACE;"
             >
             <PARAM
                 name="name1"
                 help="Domain names"
                 ptype="STRING"
                 yang_name="/dns-resolver/search=${name1}"
                 >
                 <PARAM
                     name="name2"
                     help="Domain names"
                     ptype="STRING"
                     yang_name="/dns-resolver/search=${name2}"
                     db_flag="SKIP_TREE"
                     optional="true"
                     >
                     <PARAM
                         name="name3"
                         help="Domain names"
                         ptype="STRING"
                         yang_name="/dns-resolver/search=${name3}"
                         db_flag="SKIP_TREE"
                         optional="true"
                         >
                         <PARAM
                             name="name4"
                             help="Domain names"
                             ptype="STRING"
                             yang_name="/dns-resolver/search=${name4}"
                             db_flag="SKIP_TREE"
                             optional="true"
                             >
                             <PARAM
                                 name="name5"
                                 help="Domain names"
                                 ptype="STRING"
                                 yang_name="/dns-resolver/search=${name5}"
                                 db_flag="SKIP_TREE"
                                 optional="true"
                                 >
                                 <PARAM
                                     name="name6"
                                     help="Domain names"
                                     ptype="STRING"
                                     yang_name="/dns-resolver/search=${name6}"
                                     db_flag="SKIP_TREE"
                                     optional="true"
                                     >
                                 </PARAM>
                             </PARAM>
                         </PARAM>
                     </PARAM>
                 </PARAM>
             </PARAM>
            <ACTION
                builtin="clish_extn"
                />
         </COMMAND>

         <COMMAND
             name="no ip domain-list"
             help="Remove domain names which are used to complete unqualified host name"
             operation="edit-config"
             namespace="&NAME_SPACE;"
             >
             <PARAM
                 name="name1"
                 help="Domain names"
                 ptype="STRING"
                 sub_oper_yang="remove:search"
                 yang_name="/dns-resolver/search=${name1}"
                 >
                 <PARAM
                     name="name2"
                     help="Domain names"
                     ptype="STRING"
                     sub_oper_yang="remove:search"
                     yang_name="/dns-resolver/search=${name2}"
                     db_flag="SKIP_TREE"
                     optional="true"
                     >
                     <PARAM
                         name="name3"
                         help="Domain names"
                         ptype="STRING"
                         sub_oper_yang="remove:search"
                         yang_name="/dns-resolver/search=${name3}"
                         db_flag="SKIP_TREE"
                         optional="true"
                         >
                         <PARAM
                             name="name4"
                             help="Domain names"
                             ptype="STRING"
                             sub_oper_yang="remove:search"
                             yang_name="/dns-resolver/search=${name4}"
                             db_flag="SKIP_TREE"
                             optional="true"
                             >
                             <PARAM
                                 name="name5"
                                 help="Domain names"
                                 ptype="STRING"
                                 sub_oper_yang="remove:search"
                                 yang_name="/dns-resolver/search=${name5}"
                                 db_flag="SKIP_TREE"
                                 optional="true"
                                 >
                                 <PARAM
                                     name="name6"
                                     help="Domain names"
                                     ptype="STRING"
                                     sub_oper_yang="remove:search"
                                     yang_name="/dns-resolver/search=${name6}"
                                     db_flag="SKIP_TREE"
                                     optional="true"
                                     >
                                 </PARAM>
                             </PARAM>
                         </PARAM>
                     </PARAM>
                 </PARAM>
             </PARAM>
            <ACTION
                builtin="clish_extn"
                />
         </COMMAND>

         <COMMAND
             name="ip name-server"
             help="Specify addess of name server to use; Name servers are limited to maximum of 3"
             operation="edit-config"
             namespace="&NAME_SPACE;"
             >
             <PARAM
                 name="ip-addr1"
                 help="Domain name server IP address"
                 ptype="IPV4V6_ADDR"
                 yang_name="/dns-resolver/server=${ip-addr1}"
                 >
                 <PARAM
                     name="ip-addr2"
                     help="Domain server IP address"
                     ptype="IPV4V6_ADDR"
                     yang_name="/dns-resolver/server=${ip-addr2}"
                     db_flag="SKIP_TREE"
                     optional="true"
                     >
                     <PARAM
                         name="ip-addr3"
                         help="Domain server IP address"
                         ptype="IPV4V6_ADDR"
                         yang_name="/dns-resolver/server=${ip-addr3}"
                         db_flag="SKIP_TREE"
                         optional="true"
                         >
                     </PARAM>
                 </PARAM>
             </PARAM>
            <ACTION
                builtin="clish_extn"
                />
         </COMMAND>

         <COMMAND
             name="no ip name-server"
             help="Remove the address of domain name server"
             operation="edit-config"
             namespace="&NAME_SPACE;"
             >
             <PARAM
                 name="ip-addr1"
                 help="Domain server IP address"
                 ptype="IPV4V6_ADDR"
                 sub_oper_yang="remove:server"
                 yang_name="/dns-resolver/server=${ip-addr1}"
                 >
                 <PARAM
                     name="ip-addr2"
                     help="Domain server IP address"
                     ptype="IPV4V6_ADDR"
                     sub_oper_yang="remove:server"
                     yang_name="/dns-resolver/server=${ip-addr2}"
                     db_flag="SKIP_TREE"
                     optional="true"
                     >
                     <PARAM
                         name="ip-addr3"
                         help="Domain server IP address"
                         ptype="IPV4V6_ADDR"
                         sub_oper_yang="remove:server"
                         yang_name="/dns-resolver/server=${ip-addr3}"
                         db_flag="SKIP_TREE"
                         optional="true"
                         >
                     </PARAM>
                 </PARAM>
             </PARAM>
            <ACTION
                builtin="clish_extn"
                />
         </COMMAND>

         <COMMAND
             name="ip host"
             help="Add an entry to the ip hostname table"
             operation="edit-config"
             yang_key="/dns-resolver/host/host-name"
             namespace="&NAME_SPACE;"
             >
             <PARAM
                 name="name"
                 help="Name of host"
                 ptype="HOSTNAME_STR_64"
                 yang_name="/dns-resolver/host/host-name=${name}"
                 >
                 <PARAM
                     name="ip-addr"
                     help="Host IP address"
                     ptype="IPV4V6_ADDR"
                     yang_name="/dns-resolver/host/host-ip-addr=${ip-addr}"
                     >
                 </PARAM>
             </PARAM>
            <ACTION
                builtin="clish_extn"
                />
         </COMMAND>

         <COMMAND
             name="no ip host"
             help="Remove an entry to the ip hostname table"
             operation="edit-config"
             yang_key="/dns-resolver/host/host-name"
             namespace="&NAME_SPACE;"
             >
             <PARAM
                 name="name"
                 help="Name of host"
                 ptype="HOSTNAME_STR_64"
                 sub_oper_yang="remove:host"
                 yang_name="/dns-resolver/host/host-name=${name}"
                 >
                 <PARAM
                     name="ip-addr"
                     help="Host IP address"
                     ptype="IPV4V6_ADDR"
                     optional="true"
                     >
                 </PARAM>
             </PARAM>
            <ACTION
                builtin="clish_extn"
                />
         </COMMAND>
     </VIEW>
</CLISH_MODULE>
