<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY MODULE-NS      "http://www.dellemc.com/networking/os10/dell-lacp">
<!ENTITY MODULE         "dell-lacp">
<!ENTITY SUBTREE_FILTER "Filter_Type:SUBTREE">
]>
<CLISH_MODULE
          xmlns="http://www.dellemc.com/force10/XMLSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xi="http://www.w3.org/2001/XInclude"
          xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                              http://www.dellemc.com/force10/XMLSchema/clish.xsd">

<!-- ********************************************************************** -->
<!-- *  CLEAR LACP COMMANDS                                               * -->
<!--
clear lacp counters [interface port-channel channel-number]
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->

    <VIEW name="enable-view">
        <COMMAND name="clear lacp"
                 help="Clear lacp related information"
                 ptype="SUBCOMMAND"
                 operation="generic-rpc"
                 namespace="http://www.dellemc.com/networking/os10/dell-lacp"
                 >
                 <PARAM name="counters"
                      help="clear lacp counters"
                        ptype="SUBCOMMAND"
                       mode="subcommand"
                      yang_name="/clear_lacp/counters">
    <PARAM
        name="interface"
        help="Select an interface"
        ptype="SUBCOMMAND"
        optional="true"
        mode="subcommand"
        >
            <PARAM
                name="port-channel"
                help="Port-chennal interface"
                ptype="SUBCOMMAND"
                mode="subcommand"
                >
                <PARAM
                    name="iface_num"
                    help="Port-channel number"
                    ptype="LAG_NUM"
                    yang_name="/clear_lacp/port-channel-number=port-channel${iface_num}"
                    />
            </PARAM>
  </PARAM>
                 </PARAM>
        <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>

</CLISH_MODULE>
