<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY IPV4_ADDRESS    "(^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$)">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
    http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->
    <PTYPE
        name="IP_ADDR"
        pattern="(&IPV4_ADDRESS;)"
        help="A.B.C.D"
        />
    <!--=======================================================-->
    <PTYPE
        name="UINT16"
        method="integer"
        pattern="0..65535"
        help=""
        />
    <!--=======================================================-->
    <PTYPE
        name="UINT"
        pattern="[0-9]+"
        help="Unsigned integer"
        />
    <!--=======================================================-->
    <PTYPE
        name="SUBCOMMAND"
        pattern="[^\]+"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING"
        pattern="[^|]+"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_20"
        pattern="(^([a-zA-Z0-9_-]){1,20})$"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_WITH_PIPE"
        pattern=".+"
        help="String"
        />
    <!--=======================================================-->
    <PTYPE
        name="IP_ADDR_MASK"
        pattern="(((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))(/(1[0-9]|2[0-9]|3[0-2]|[0-9]))"
        help="A.B.C.D/mask"
        />
    <!--=======================================================-->
    <PTYPE
        name="MAC_ADDR"
        pattern="[0-9a-fA-F]{2}(:[0-9a-fA-F]{2}){5}"
        help="nn:nn:nn:nn:nn:nn"
        preprocess="tolower"
        />
    <!--=======================================================-->
    <PTYPE
        name="STRING_1_140"
        pattern="(([^,=|\&quot;&amp;])|(\w)){1,140}"
        help="String"
        />
    <!--=======================================================-->
</CLISH_MODULE>
