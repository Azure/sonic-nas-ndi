<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY SUBTREE_FILTER "Filter_Type:SUBTREE">
<!ENTITY MODULE-NS      "dell-dhcp=http://www.dellemc.com/networking/os10/dell-dhcp">
<!ENTITY IF-MODULE-NS   "urn:ietf:params:xml:ns:yang:ietf-interfaces">
<!ENTITY MODULE-PREF-IANA-IF "ianaift">
<!ENTITY MODULE-NS-IANA-IF "&MODULE-PREF-IANA-IF;=urn:ietf:params:xml:ns:yang:iana-if-type">
<!ENTITY MODULE-PREF-IF "dell-if">
<!ENTITY MODULE-NS-IF "&MODULE-PREF-IF;=http://www.dellemc.com/networking/os10/dell-interface">
<!ENTITY MODULE-PREF-DELL-IP "dell-ip">
<!ENTITY MODULE-NS-DELL-IP  "http://www.dellemc.com/networking/os10/dell-ip">
<!ENTITY MODULE-INTF-IP-NS  "&MODULE-PREF-DELL-IP;=&MODULE-NS-DELL-IP;">
<!ENTITY MODULE-NS-IETF-IF "&IF-MODULE-NS;,&MODULE-NS-IANA-IF;,&MODULE-NS-IF;,&MODULE-INTF-IP-NS;">
<!ENTITY FILTER_IETF "Filter_Type:SUBTREE;Filter_Namespace:&MODULE-NS-IETF-IF;;Filter_Conditions:">
<!ENTITY FILTER_NS "Filter_Namespace:http://www.dellemc.com/networking/os10/dell-dhcp">
<!ENTITY INTF-STATES-INTF   "/interfaces-state/interface">
<!ENTITY DHCP_CLIENT_PKT_STATS  "&INTF-STATES-INTF;/dell-dhcp:dhcp-client-pkt-statistics">
<!ENTITY DHCP_LEASE_OPER    "&INTF-STATES-INTF;/dell-dhcp:dhcp-lease-oper">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd">
<!-- ********************************************************************** -->
<!-- *  SHOW DHCP COMMANDS                                                 * -->
<!--
show ip dhcp binding .
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->
	<VIEW name="enable-view">
		<COMMAND name="show ip dhcp binding"
			help="DHCP binding details"
			namespace="http://www.dellemc.com/networking/os10/dell-dhcp"
            operation="get"
			>
			<ACTION builtin="clish_file_print"> /opt/dell/os10/lib/show_binding</ACTION>
		</COMMAND>
		<COMMAND name="show ip dhcp"
			help="DHCP information"
			namespace="&MODULE-NS;"
			operation="get"
			>
            <PARAM
                help="switch subcommand"
                name="type"
                mode="switch"
                ptype="SUBCOMMAND">

            <PARAM
                help="Show DHCP client"
                name="client"
                ptype="SUBCOMMAND"
                pfeature="NOT_SUPPORTED"
                mode="subcommand">
                    <PARAM
                        help="Show client statistics"                                                                                                          name="statistics"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                           <PARAM
                               name="iface-name"
                               help="Interface name"
                               ptype="PHY_VL_PO_INTERFACE"
                               yang_filter="&SUBTREE_FILTER;;Filter_Conditions:&INTF-STATES-INTF;/name=${iface-name},&DHCP_CLIENT_PKT_STATS;;Filter_Namespace:&IF-MODULE-NS;,&MODULE-NS;"
                               template="show_ip_dhcp_cl_statistics.xsl">
                           </PARAM>
                    </PARAM>
            </PARAM>
            <PARAM name="lease"
                help="DHCP lease details"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_filter="&SUBTREE_FILTER;;Filter_Conditions:&INTF-STATES-INTF;,&DHCP_LEASE_OPER;;Filter_Namespace:&IF-MODULE-NS;,&MODULE-NS;"
                template="show_ip_dhcp_lease.xsl"
                >
           </PARAM>
           </PARAM>
			<ACTION builtin="clish_extn"> </ACTION>
		</COMMAND>
		<COMMAND name="show ip interface"
			help="Show interface information"
			namespace="&MODULE-INTF-IP-NS;"
			operation="get"
			>
            <PARAM
                name="brief"
                help="Show interface information in brief"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_filter="&FILTER_IETF;/interfaces/interface/dell-ip:ipv4|&FILTER_IETF;&INTF-STATES-INTF;/admin-status,&INTF-STATES-INTF;/oper-status,&INTF-STATES-INTF;/dell-ip:ipv4-info"
                template="show_ip_interface_brief.xsl">
            </PARAM>
			<ACTION builtin="clish_extn"> </ACTION>
		</COMMAND>
		<COMMAND name="show ipv6 dhcp"
			help="DHCPV6 information"
			namespace="http://www.dellemc.com/networking/os10/dell-dhcp"
			>
			<PARAM name="binding"
				help="DHCPV6 binding details"
				ptype="SUBCOMMAND"
				mode="subcommand"
				>
			</PARAM>
			<ACTION builtin="clish_file_print"> /opt/dell/os10/lib/show_binding_ipv6</ACTION>
		</COMMAND>
	</VIEW>
</CLISH_MODULE>
