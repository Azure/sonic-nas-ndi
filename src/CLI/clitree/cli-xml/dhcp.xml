    <!-- ********************************************************************** -->
    <!-- *  CONFIGURATION COMMANDS                                            * -->
    <!-- *  View : configure-view                                             * -->
    <!-- ********************************************************************** -->
    <VIEW
        name="configure-view"
        >
        <COMMAND
            name="ip dhcp"
            help="Dynamic host control protocol"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            view="configure-dhcp-view"
            mode="subcommand"
            >
        <PARAM
            name="server"
            help="DHCP Server"
            operation="edit-config"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
            viewid="afview=configure-dhcp-view"
            yang_name="/server"
            />
            <ACTION
               builtin="clish_extn"
               > </ACTION>
        </COMMAND>
        <COMMAND
            name="ip dhcp relay"
            help="DHCP Relay"
            pfeature="NOT_SUPPORTED"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            mode="subcommand"
            >
        </COMMAND>
        <COMMAND
            name="ip dhcp relay information-option"
            help="Enable the information option"
            pfeature="NOT_SUPPORTED"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            mode="subcommand"
            >
        <PARAM
            name="relay-options"
            help="Enable relay information options"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM
                name="trust-downstream"
                help="Enable the trust-downstream"
                operation="edit-config"
                namespace="&NAME_SPACE;"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&DHCP_RELAY;/dhcp-relay-inf-option/trust-down-stream/enable=true"
                />
            <PARAM
                name="remote-id"
                help="Enable remote-id string in option-82"
                ptype="SUBCOMMAND"
                mode="subcommand"
                >
                <PARAM
                    name="remote-id-options"
                    help="Enable remote-id string in option-82"
                    ptype="SUBCOMMAND"
                    mode="switch"
                    >
                    <PARAM
                        name="hostname"
                        help="Set hostname as remote-id in option-82"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&DHCP_RELAY;/dhcp-relay-inf-option/remote-id/id=hostname"
                        />
                    <PARAM
                        name="mac"
                        help="Set chassis mac as remote-id in option-82"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&DHCP_RELAY;/dhcp-relay-inf-option/remote-id/id=mac"
                        />
                    <PARAM
                        name="WORD"
                        help="Remote-id string (Option-82)"
                        ptype="STRING"
                        yang_name="&DHCP_RELAY;/dhcp-relay-inf-option/remote-id/remote-id=str,&DHCP_RELAY;/dhcp-relay-inf-option/remote-id/str=${WORD}"
                        />
                </PARAM>
            </PARAM>
            </PARAM>
            <ACTION
                    builtin="clish_extn"
             > </ACTION>
        </COMMAND>
        <COMMAND
            name="no ip dhcp"
            help="Dynamic host control protocol"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            mode="subcommand"
            >
        <PARAM
            name="server"
            help="DHCP Server"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
            sub_oper_yang="remove:server"
            yang_name="/server"
            />
            <ACTION
               builtin="clish_extn"
               > </ACTION>
        </COMMAND>
	<COMMAND
            name="no ip dhcp relay"
            help="DHCP Relay"
            pfeature="NOT_SUPPORTED"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            mode="subcommand"
            >
        </COMMAND>

        <COMMAND
            name="no ip dhcp relay information-option"
            help="Enable the information option"
            pfeature="NOT_SUPPORTED"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            mode="subcommand"
            >
        <PARAM
            name="relay-options"
            help="Enable relay information options"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM
                name="trust-downstream"
                help="Enable the trust-downstream"
                operation="edit-config"
                namespace="&NAME_SPACE;"
                sub_oper_yang="remove:trust-down-stream"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&DHCP_RELAY;/dhcp-relay-inf-option/trust-down-stream"
                />
            <PARAM
                name="remote-id"
                help="Enable remote-id string in option-82"
                ptype="SUBCOMMAND"
                mode="subcommand"
                sub_oper_yang="remove:id"
                yang_name="&DHCP_RELAY;/dhcp-relay-inf-option/remote-id/id"
                >
            </PARAM>
        </PARAM>
        <ACTION
                builtin="clish_extn"
        > </ACTION>
        </COMMAND>

    </VIEW>
    <VIEW
        name="configure-dhcp-view"
        prompt="${SYSTEM_NAME}(config-dhcp)# "
        depth="2"
        >
        <!-- Inheritance -->
        <NAMESPACE
            ref="configure-view"
            help="false"
            completion="false"
            />

        <COMMAND
            name="show"
            help="Show configuration"
            />
		<COMMAND
            name="show configuration"
            help="Current configuration (default = running)"
            yang_filter="&DHCP_NS_FILTER;;&SUBTREE_FILTER;;Filter_Conditions:/server/global-ip-pool">
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="pool"
            help="DHCP Pool Configuration"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            view="configure-dhcp-pool-view"
            viewid="name=${pname}"
	    yang_key="&DHCP_POOL_KEY;"
	    >
            <PARAM
	        name="pname"
	        help="DHCP server pool name"
            	ptype="STRING"
	        yang_name="&DHCP_SERVER_POOL;/pool-name=${pname}"
            />	
            <ACTION
                builtin="clish_extn"
                > </ACTION>
	</COMMAND>
	<COMMAND
		name="no"
		help="DHCP Pool unconfig"
		operation="edit-config"/>
	<COMMAND
		name="no pool"
		help="DHCP Pool unconfig"
		operation="edit-config"
		namespace="&NAME_SPACE;"
		ptype="SUBCOMMAND"
 		mode="subcommand"
	        yang_key="&DHCP_POOL_KEY;"
                yang_name="&DHCP_SERVER_POOL;/pool-name"
		>
	    <PARAM
	        name="pname"
	        help="DHCP server pool name"
            	ptype="STRING"
        	sub_oper_yang="remove:ip-pool-list"
	        yang_name="&DHCP_SERVER_POOL;/pool-name=${pname}"
            />	
	    <ACTION
		builtin="clish_extn"
		> </ACTION>
	</COMMAND>
	<COMMAND
	    name="no disable"
        help="Enable DHCP server"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
        yang_name="/server/disable=false"
        add_config_entry="true"
	    >
	    <ACTION
		builtin="clish_extn"
		> </ACTION>
	</COMMAND>

	<COMMAND
	    name="disable"
        help="Disable DHCP server"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    yang_name="/server/disable=true"
	    >
	    <ACTION
		builtin="clish_extn"
		> </ACTION>
	</COMMAND>
    </VIEW>
    <!-- ********************************************************************** -->
    <!-- *  CONFIGURATION COMMANDS                                            * -->
    <!-- *  View : configure-dhcp-view                                  * -->
    <!-- ********************************************************************** -->
    <VIEW
        name="configure-dhcp-pool-view"
        prompt="${SYSTEM_NAME}(config-dhcp-${name})# "
        depth="3"
        >
        <!-- Inheritance
        <NAMESPACE
            ref="configure-dhcp-view"
            help="false"
            completion="false"
            />
	-->
        <COMMAND
            name="show"
            help="Show configuration"
            />
		<COMMAND
            name="show configuration"
            help="Current configuration (default = running)"
            yang_filter="&DHCP_NS_FILTER;;&SUBTREE_FILTER;;Filter_Conditions:&DHCP_POOL_KEY;=${name}">
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
	<COMMAND
	    name="lease"
	    help="Lease for each client"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    mode="subcommand"
            yang_name="&DHCP_POOL_KEY;=${name}"
	    >
        <PARAM
            name="lease-option"
            help="lease config options"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM
                name="infinite"
                help="Specifies that the lease never expires"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="&DHCP_SERVER_POOL;/lease-time/infinite=true"
                />
            <PARAM
                name="days"
                help="Number of days of the lease [0-31]"
                ptype="RANGE_0_31"
                yang_name="&DHCP_SERVER_POOL;/lease-time/lease_hours=0,&DHCP_SERVER_POOL;/lease-time/lease_mins=0,&DHCP_SERVER_POOL;/lease-time/lease_days=${days}"
                >
                <PARAM
                    name="hours"
                    help="Number of hours of the lease [0-23]"
                    ptype="RANGE_0_23"
                    optional="true"
                    refresh_yang_name="true"
                    yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/lease-time/lease_days=${days},&DHCP_SERVER_POOL;/lease-time/lease_mins=0,&DHCP_SERVER_POOL;/lease-time/lease_hours=${hours}"
                    >
                    <PARAM
                        name="minutes"
                        help="Number of minutes of the lease [0-59]"
                        ptype="RANGE_0_59"
                        optional="true"
                        refresh_yang_name="true"
                        yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/lease-time/lease_days=${days},&DHCP_SERVER_POOL;/lease-time/lease_hours=${hours},&DHCP_SERVER_POOL;/lease-time/lease_mins=${minutes}"
                        />
		</PARAM>
	   </PARAM>
        </PARAM>
        <ACTION
            builtin="clish_extn"
            > </ACTION>
	</COMMAND>
	<COMMAND
		name="no"
		help="DHCP Pool unconfig"
		operation="edit-config"/>
	<COMMAND
	    name="no lease"
	    help="Lease for each client"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    mode="subcommand"
            yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/lease-time/infinite=false"
	    >
        <ACTION
            builtin="clish_extn"
        > </ACTION>
	</COMMAND>

	<COMMAND
	    name="network"
	    help="Configure subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    ptype="SUBCOMMAND"
	    mode="subcommand"
	    yang_name="&DHCP_POOL_KEY;=${name}"
	   >
	   <PARAM
			name="net_ip"
			help="Range of addresses"
			ptype="IP_ADDR_DHCP_SUBNET_IPV4IPV6"
			yang_name="&DHCP_SERVER_POOL;/ip-range=${net_ip}"
			/>
		<ACTION
 			builtin="clish_extn"
		> </ACTION>
	</COMMAND>
	<COMMAND
	    name="no network"
	    help="Configure subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    ptype="SUBCOMMAND"
        mode="subcommand"
        yang_name="&DHCP_POOL_KEY;=${name}"
        >
            <PARAM
                name="net_ip"
                help="Range of addresses"
                ptype="IP_ADDR_DHCP_SUBNET_IPV4IPV6"
                sub_oper_yang="remove:ip-range"
                yang_name="&DHCP_SERVER_POOL;/ip-range=${net_ip}"
                />

            <ACTION
                builtin="clish_extn"
                > </ACTION>
        </COMMAND>

	<COMMAND
	    name="default-router"
	    help="Default router for this subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    ptype="SUBCOMMAND"
            yang_name="&DHCP_POOL_KEY;=${name}"
	    mode="subcommand"
	   >
	   <PARAM
	       name="router1"
	       help="IP address"
	       ptype="IP_ADDR"
	       recursive="true"
	       yang_name="&DHCP_SERVER_POOL;/dhcp-options/router=${router1}"
	   />
           <ACTION
              builtin="clish_extn"
             > </ACTION>
	</COMMAND>
	<COMMAND
	    name="no default-router"
	    help="Default router for this subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    ptype="SUBCOMMAND"
	    yang_name="&DHCP_POOL_KEY;=${name}"
	    mode="subcommand"
	   >
	   <PARAM
	   	name="router1"
	   	help="IP address"
	   	ptype="IP_ADDR"
	  	recursive="true"
                sub_oper_yang="remove:router"
	   	yang_name="&DHCP_SERVER_POOL;/dhcp-options/router=${router1}"
	   	/>
        <ACTION
              builtin="clish_extn"
             > </ACTION>
	</COMMAND>


        <!-- domain name -->
	<COMMAND
	    name="domain-name"
	    help="Domain name for this subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    mode="subcommand"
	   >
	   <PARAM
                 name="dname"
                 help="Domain name string (max length 31)"
                 ptype="STRING"
  	         yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/dhcp-options/domain-name=${dname}"
                 />
           <ACTION
                builtin="clish_extn"
                > </ACTION>

	</COMMAND>
	<COMMAND
	    name="no domain-name"
	    help="Domain name for this subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
            sub_oper_yang="remove:domain-name"
  	    yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/dhcp-options/domain-name"
	   >
        <ACTION
            builtin="clish_extn"
         > </ACTION>

	</COMMAND>

	<COMMAND
	    name="dns-server"
	    help="Dmain name server IP for this subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    yang_name="&DHCP_POOL_KEY;=${name}"
	    mode="subcommand"
	   >
	   <PARAM
       	       name="address1"
               help="IP address"
               ptype="IPV4V6_ADDR"
	       recursive="true"
  	       yang_name="&DHCP_SERVER_POOL;/dhcp-options/domain-name-server=${address1}"
            />

            <ACTION builtin="clish_extn"
                  >
            </ACTION>

	</COMMAND>
	<COMMAND
	    name="no dns-server"
	    help="Dmain name server IP for this subnet"
	    operation="edit-config"
	    namespace="&NAME_SPACE;"
	    yang_name="&DHCP_POOL_KEY;=${name}"
	    mode="subcommand"
	   >
           <PARAM
               name="address1"
               help="IP address"
               ptype="IPV4V6_ADDR"
	       recursive="true"
	       sub_oper_yang="remove:domain-name-server"
  	       yang_name="&DHCP_SERVER_POOL;/dhcp-options/domain-name-server=${address1}"
            />
       <ACTION builtin="clish_extn"
             >
       </ACTION>
	</COMMAND>


	<COMMAND
            name="netbios-node-type"
            help="NetBIOS node type"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            mode="subcommand"
            >
            <PARAM
                name="asnotation-commands"
                help="asnotation-sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="Broadcast"
                    help="Enter the keyword b-node"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/dhcp-options/nb-node-type=b-node"
                    />
                <PARAM
                    name="Hybrid"
                    help="Enter the keyword h-node"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/dhcp-options/nb-node-type=h-node"
                    />
		 <PARAM
                    name="Mixed"
                    help="Enter the keyword m-node"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/dhcp-options/nb-node-type=m-node"
                    />
		 <PARAM
                    name="Peer-to-peer"
                    help="Enter the keyword p-node"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&DHCP_POOL_KEY;=${name},&DHCP_SERVER_POOL;/dhcp-options/nb-node-type=p-node"
                    />
	    </PARAM>
            <ACTION
                builtin="clish_extn"
