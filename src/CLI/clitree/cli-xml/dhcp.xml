    <VIEW name="configure-if-view">
        <COMMAND
            name="ip helper-address"
            help="Dynamic host control protocol relay"
            operation="edit-config"
            namespace="&MODULE-INTF-DHCP-NS;"
            yang_key="&INTF_ENTRY;/dell-dhcp:dhcp-relay-if-cfgs/dell-dhcp:server-address"
            yang_name="&INTF_NAME;"
            db_multi_yang_name=""
            >
            <PARAM
                name="dhcp-server-addr"
                help="Forwards UDP broadcasts to dhcp server"
                ptype="IP_ADDR"
                recursive="true"
                yang_name="&INTF_ENTRY;/dell-dhcp:dhcp-relay-if-cfgs/dell-dhcp:server-address=${dhcp-server-addr}"
                />
            <ACTION
                builtin="clish_extn"
                > </ACTION>
        </COMMAND>
        <COMMAND
            name="no ip helper-address"
            help="Delete the helper address"
            namespace="&MODULE-INTF-DHCP-NS;"
            yang_name="&INTF_NAME;"
            operation="edit-config">
            <PARAM
                name="dhcp-server-addr"
                help="Forwards UDP broadcasts to dhcp server"
                ptype="IP_ADDR"
                recursive="true"
                sub_oper_yang="remove:server-address"
                yang_name="&INTF_ENTRY;/dell-dhcp:dhcp-relay-if-cfgs/dell-dhcp:server-address=${dhcp-server-addr}"
                />
            <ACTION
                builtin="clish_extn"
                > </ACTION>
        </COMMAND>
    </VIEW>
