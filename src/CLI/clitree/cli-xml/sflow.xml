<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY SUBTREE_FILTER                     "Filter_Type:SUBTREE">
<!ENTITY SFLOW_CONFIG                       "/sflow-config">
<!ENTITY SFLOW_INTF                         "/config">
<!ENTITY SFLOW_COLLECTOR                    "&SFLOW_CONFIG;/collector">
<!ENTITY SFLOW_OPER_DATA                    "/oper-data">
<!ENTITY SFLOW_OPER_DATA_STATISTICS         "&SFLOW_OPER_DATA;/statistics">
<!ENTITY SFLOW_NS                           "http://www.dellemc.com/networking/os10/dell-sflow">

<!ENTITY SFLW-PREF "dell-sflow">
<!ENTITY SFLW_NS "&SFLW-PREF;=http://www.dellemc.com/networking/os10/dell-sflow">
<!ENTITY SFLW_INTF_NS "urn:ietf:params:xml:ns:yang:ietf-interfaces,&SFLW_NS;">
<!ENTITY SFLW_INTF "/interfaces/interface">
<!ENTITY SFLW_STATE_INTF "/interfaces-state/interface">
<!ENTITY SFLW_RANGE_INTF "/interfaces/interface[]">
<!ENTITY SFLW_OPER_DATA_INTF "&SFLW_STATE_INTF;/&SFLW-PREF;:oper-data">
<!ENTITY SFLW_INTF_INFO "&SFLW_INTF;/&SFLW-PREF;:config">
<!ENTITY SFLW_RANGE_INTF_INFO "&SFLW_RANGE_INTF;/&SFLW-PREF;:config">
<!ENTITY SFLW_INTF_ENABLE "&SFLW_INTF_INFO;/&SFLW-PREF;:enable">
<!ENTITY SFLW_INTF_DIRECTION "&SFLW_INTF_INFO;/&SFLW-PREF;:direction">
<!ENTITY SFLW_RANGE_INTF_ENABLE "&SFLW_RANGE_INTF_INFO;/&SFLW-PREF;:enable">
<!ENTITY SFLW_RANGE_INTF_DIRECTION "&SFLW_RANGE_INTF_INFO;/&SFLW-PREF;:direction">
<!ENTITY SFLW_IFACE "&SFLW_INTF;/name">
<!ENTITY SFLW_RANGE_IFACE "&SFLW_RANGE_INTF;/name">
<!ENTITY SFLW_STATE_IFACE "&SFLW_STATE_INTF;/name">
<!ENTITY FILTER_NS "Filter_Namespace:&SFLW_INTF_NS;,&SFLOW_NS;">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->
    <VIEW
        name="enable-view"
        >
        <!-- show candidate-configuration sflow -->
        <COMMAND
            name="show candidate-configuration sflow"
            help="Current candidate sFlow configuration"
            yang_filter="&SUBTREE_FILTER;;Filter_Conditions:&SFLOW_CONFIG;;Filter_Namespace:&SFLOW_NS;|&SUBTREE_FILTER;;Filter_Conditions:&SFLW_INTF_INFO;;&FILTER_NS;"
            db_mode="configure,configure-if,configure-lag"
            >
            <ACTION
               builtin="clish_extn"
               />
        </COMMAND>
        <!-- show running-configuration sflow -->
        <COMMAND
            name="show running-configuration sflow"
            help="Current running sFlow configuration"
            yang_filter="&SUBTREE_FILTER;;Filter_Conditions:&SFLOW_CONFIG;;Filter_Namespace:&SFLOW_NS;|&SUBTREE_FILTER;;Filter_Conditions:&SFLW_INTF_INFO;;&FILTER_NS;"
            db_mode="configure,configure-if,configure-lag"
            >
            <ACTION
               builtin="clish_extn"
               />
        </COMMAND>
        <!-- show sflow -->
        <COMMAND
            name="show sflow"
            help="Show sflow"
            operation="get"
            namespace="&SFLOW_NS;">
            <ACTION builtin="clish_file_print">/opt/dell/os10/bin/show_sflow.py</ACTION>
        </COMMAND>
        <!-- show sflow interface <if_name> -->
        <COMMAND
            name="show sflow interface"
            help="Show sflow"
            operation="get"
            namespace="&SFLOW_NS;"
            template="show_sflow_int.xsl"
            >
            <PARAM
                name="interface-switch"
                help="Select an interface"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="ifacename"
                    help="physical ethernet interface"
                    ptype="PHY_INTERFACE"
                    mode="subcommand"
                    yang_filter="&SUBTREE_FILTER;;Filter_Conditions:&SFLW_STATE_IFACE;=${ifacename},&SFLW_OPER_DATA_INTF;;&FILTER_NS;"
                    />
                <PARAM
                    name="port-channeliface"
                    help="port channel interface"
                    ptype="PO_INTERFACE"
                    mode="subcommand"
                    yang_filter="&SUBTREE_FILTER;;Filter_Conditions:&SFLW_STATE_IFACE;=${port-channeliface},&SFLW_OPER_DATA_INTF;;&FILTER_NS;"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
    </VIEW>
    <!--=======================================================-->
    <!--                Config MODE                            -->
    <!--=======================================================-->
    <VIEW
        name="configure-view"
        >

        <COMMAND
            name="sflow"
            help="Configure sflow parameters"
            operation="edit-config"
            namespace="&SFLOW_NS;"
            >
            <PARAM
                name="sflow-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <!-- sflow enable {all-interfaces} -->
                <PARAM
                    name="enable"
                    help="Enables sflow globally"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLOW_CONFIG;/enable=true"
                    >
                    <PARAM
                        name="all-interfaces"
                        help="Enables sflow globally"
                        optional="true"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="&SFLOW_CONFIG;/enable-all=true"
                        />
                </PARAM>
                <!-- sflow sample-rate [sampling-rate] -->
                <PARAM
                    name="sample-rate"
                    help="Sample rate for sampling sFlow packets"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="rate"
                        help="Sampling rate (Default value = 32768)"
                        ptype="RANGE_4096_65535"
                        yang_name="&SFLOW_CONFIG;/sample-rate=${rate}"
                        />
                </PARAM>
                <!-- sflow polling-interval [poll-interval] -->
                <PARAM
                    name="polling-interval"
                    help="Poll interval for sampling sFlow packets"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="interval"
                        help="Counter poll interval (Default value = 30)"
                        ptype="RANGE_10_300"
                        yang_name="&SFLOW_CONFIG;/polling-interval=${interval}"
                        />
                </PARAM>
                <!-- sflow max-header-size [header-size] -->
                <PARAM
                    name="max-header-size"
                    help=" Maximum header size for sampling sFlow packets"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="size"
                        help="Header size (Default value = 128)"
                        ptype="RANGE_64_256"
                        yang_name="&SFLOW_CONFIG;/max-header-size=${size}"
                        />
                </PARAM>
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
        <!-- sflow collector {ip-address | ipv6-address} agent-addr {ip-address | ipv6-address} [ {port number} max-datagram-size {datagram-size} vrf { managment | default } ] -->
        <COMMAND
            name="sflow collector"
            help="IP address for sFlow Collector"
            operation="edit-config"
            namespace="&SFLOW_NS;"
            yang_key="&SFLOW_COLLECTOR;/ip-address,&SFLOW_COLLECTOR;/port"
            >
            <PARAM
                name="ip"
                help="IP Address"
                ptype="IPV4V6_ADDR"
                yang_name="&SFLOW_COLLECTOR;/ip-address=${ip}"
                >
                <PARAM
                    name="agent-addr"
                    help="IP address for sFlow agent"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="agentip"
                        help="IP Address"
                        prompt_fn="different_agent_ip_configured"
                        prompt_str="${agentip}"
                        ptype="IPV4V6_ADDR"
                        yang_name="&SFLOW_COLLECTOR;/port=${portno},&SFLOW_COLLECTOR;/agent-ip=${agentip}"
                        />
                </PARAM>
                <PARAM
                    name="portno"
                    help="Collector port number (Default port = 6343)"
                    ptype="RANGE_1_65535"
                    optional="true"
                    default="6343"
                    refresh_yang_name="true"
                    yang_name="&SFLOW_COLLECTOR;/ip-address=${ip},&SFLOW_COLLECTOR;/agent-ip=${agentip},&SFLOW_COLLECTOR;/port=${portno}"
                    />
                <PARAM
                    name="max-datagram-size"
                    help="Maximum datagram size"
                    ptype="SUBCOMMAND"
                    optional="true"
                    mode="subcommand"
                    >
                    <PARAM
                        name="size"
                        help="Datagram size (Default value = 1400)"
                        ptype="RANGE_400_1500"
                        yang_name="&SFLOW_COLLECTOR;/max-datagram-size=${size}"
                        />
                </PARAM>
                <PARAM
                    name="vrf"
                    help="VRF Instance"
                    ptype="SUBCOMMAND"
                    optional="true"
                    mode="subcommand"
                    >
                    <PARAM
                        name="vrf-subcmds"
                        help="Sub-commands"
                        ptype="SUBCOMMAND"
                        mode="switch"
                        >
                        <PARAM
                            name="default"
                            help="Collector is in front panel port"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&SFLOW_COLLECTOR;/vrf=default"
                            />
                        <PARAM
                            name="management"
                            help="Collector is in Management interface"
                            pfeature="NOT_SUPPORTED"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&SFLOW_COLLECTOR;/vrf=management"
                            />
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>

        <!-- no sflow -->
        <COMMAND
            name="no sflow"
            help="Remove/Delete sflow parameters"
            operation="edit-config"
            namespace="&SFLOW_NS;"
            >
            <PARAM
                name="nosflow-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <!-- no sflow enable [all-interfaces]-->
                <PARAM
                    name="enable"
                    help="Disable sflow globally"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    sub_oper_yang="remove:enable,remove:enable-all"
                    yang_name="&SFLOW_CONFIG;/enable-all,&SFLOW_CONFIG;/enable"
                    >
                    <PARAM
                        name="all-interfaces"
                        help="Disable sflow global interface"
                        optional="true"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        refresh_yang_name = "true"
                        sub_oper_yang="clear;remove:enable-all"
                        yang_name="&SFLOW_CONFIG;/enable-all"
                        />
                </PARAM>
                <!-- no sflow sample-rate -->
                <PARAM
                    name="sample-rate"
                    help="Sampling rate for sampling sflow packets"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name="&SFLOW_CONFIG;/sample-rate"
                    >
                </PARAM>
                <!-- no sflow polling-interval -->
                <PARAM
                    name="polling-interval"
                    help="Poll interval for sampling sflow packets"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name="&SFLOW_CONFIG;/polling-interval"
                    >
                </PARAM>
                <!-- no sflow max-sampled-size -->
                <PARAM
                    name="max-header-size"
                    help="Maximum header size for sampling sflow packets"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name="&SFLOW_CONFIG;/max-header-size"
                    >
                </PARAM>
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
	<!-- no sflow collector {ip-address | ipv6-address} [ agent-addr {ip-address | ipv6-address} ] {port number} [ max-datagram-size {datagram-size} vrf { managment | default } ] -->
        <COMMAND
            name="no sflow collector"
            help="IP address for sFlow collector"
            operation="edit-config"
            namespace="&SFLOW_NS;"
            sub_oper_yang="remove:collector"
            yang_key="&SFLOW_COLLECTOR;/ip-address,&SFLOW_COLLECTOR;/port"
            >
            <PARAM
                name="ip"
                help="IP Address"
                ptype="IPV4V6_ADDR"
                yang_name="&SFLOW_COLLECTOR;/port=${portno},&SFLOW_COLLECTOR;/ip-address=${ip}"
                >
                <PARAM
                    name="agent-addr"
                    help="IP address for sFlow agent"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    optional="true"
                    sub_oper_yang="remove:agent-ip"
                    yang_name="&SFLOW_COLLECTOR;/agent-ip=0.0.0.0"
                    >
                    <PARAM
                        name="agentip"
                        help="IP Address"
                        ptype="IPV4V6_ADDR"
                        />
                </PARAM>
                <PARAM
                    name="portno"
                    help="Collector udp port"
                    ptype="RANGE_1_65535"
                    optional="true"
                    default="6343"
                    refresh_yang_name="true"
                    yang_name="&SFLOW_COLLECTOR;/ip-address=${ip},&SFLOW_COLLECTOR;/agent-ip=0.0.0.0,&SFLOW_COLLECTOR;/port=${portno}"
                    />
                <PARAM
                    name="max-datagram-size"
                    help="Maximum datagram size"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    optional="true"
                    sub_oper_yang="remove:max-datagram-size"
                    yang_name="&SFLOW_COLLECTOR;/max-datagram-size"
                    >
                    <PARAM
                        name="size"
                        help="Datagram size"
                        ptype="RANGE_400_1500"
                        yang_name="&SFLOW_COLLECTOR;/max-datagram-size=${size}"
                        />
                </PARAM>
                <PARAM
                    name="vrf"
                    help="VRF Instance"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    optional="true"
                    sub_oper_yang="remove:vrf"
                    yang_name="&SFLOW_COLLECTOR;/vrf"
                    >
                    <PARAM
                        name="vrf-subcmds"
                        help="sub-commands"
                        ptype="SUBCOMMAND"
                        mode="switch"
                        >
                        <PARAM
                            name="default"
                            help="Collector is in front panel port"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            />
                        <PARAM
                            name="management"
                            help="Collector is in Management interface"
                            pfeature="NOT_SUPPORTED"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            />
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
    </VIEW>
    <!--=======================================================-->
    <!--                Config Interface MODE                  -->
    <!--=======================================================-->
    <VIEW
        name="configure-if-view"
        >
        <!-- sflow [direction-] enable -->
        <COMMAND
            name="sflow"
            help="Sflow configurations in specific interface"
            operation="edit-config"
            namespace="&SFLW_INTF_NS;"
            >
            <PARAM
                name="sampling-dir-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="enable"
                    help="Enables ingress sflow in specific interface"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_IFACE;=${ifacename},&SFLW_INTF_ENABLE;=true,&SFLW_INTF_DIRECTION;=ingress"
                    />
                <PARAM
                    name="egress-enable"
                    help="Egress enable in interface"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_IFACE;=${ifacename},&SFLW_INTF_ENABLE;=true,&SFLW_INTF_DIRECTION;=egress"
                    />
                <PARAM
                    name="both-enable"
                    help="Both ingress and egress enable in interface"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_IFACE;=${ifacename},&SFLW_INTF_ENABLE;=true,&SFLW_INTF_DIRECTION;=both"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
        <!-- no sflow [direction-] enable -->
        <COMMAND
            name="no sflow"
            operation="edit-config"
            namespace="&SFLW_INTF_NS;"
            help="Deletes the sFlow configurations in specific interface"
            >
            <PARAM
                name="nosampling-dir-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="enable"
                    help="Disables ingress sflow for specific interface"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_IFACE;=${ifacename},&SFLW_INTF_DIRECTION;,&SFLW_INTF_ENABLE;=false"
                    />
                <PARAM
                    name="egress-enable"
                    help="Egress disable in specific interface"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_IFACE;=${ifacename},&SFLW_INTF_DIRECTION;,&SFLW_INTF_ENABLE;=false"
                    />
                <PARAM
                    name="both-enable"
                    help="Both ingress and egress enable in specific interface"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_IFACE;=${ifacename},&SFLW_INTF_DIRECTION;,&SFLW_INTF_ENABLE;=false"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
    </VIEW>
    <!--=======================================================-->
    <!--                Config Interface range MODE            -->
    <!--=======================================================-->
    <VIEW
        name="configure-if-range-view"
        >
        <!-- sflow [direction-] enable -->
        <COMMAND
            name="sflow"
            help="Sflow configurations in a range of interfaces"
            operation="edit-config"
            namespace="&SFLW_INTF_NS;"
            >
            <PARAM
                name="sampling-dir-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="enable"
                    help="Enables ingress sflow in a range of interfaces"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_RANGE_IFACE;=${ifacename},&SFLW_RANGE_INTF_ENABLE;=true,&SFLW_RANGE_INTF_DIRECTION;=ingress"
                    />
                <PARAM
                    name="egress-enable"
                    help="Egress enable in a range of interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_RANGE_IFACE;=${ifacename},&SFLW_RANGE_INTF_ENABLE;=true,&SFLW_RANGE_INTF_DIRECTION;=egress"
                    />
                <PARAM
                    name="both-enable"
                    help="Both ingress and egress enable in a range of interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_RANGE_IFACE;=${ifacename},&SFLW_RANGE_INTF_ENABLE;=true,&SFLW_RANGE_INTF_DIRECTION;=both"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
        <!-- no sflow [direction-] enable -->
        <COMMAND
            name="no sflow"
            operation="edit-config"
            namespace="&SFLW_INTF_NS;"
            help="Deletes the sFlow configurations from a range of interfaces"
            >
            <PARAM
                name="nosampling-dir-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="enable"
                    help="Disables ingress sflow for a range of interfaces"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_RANGE_IFACE;=${ifacename},&SFLW_RANGE_INTF_DIRECTION;,&SFLW_RANGE_INTF_ENABLE;=false"
                    />
                <PARAM
                    name="egress-enable"
                    help="Egress disable in a range of interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_RANGE_IFACE;=${ifacename},&SFLW_RANGE_INTF_DIRECTION;,&SFLW_RANGE_INTF_ENABLE;=false"
                    />
                <PARAM
                    name="both-enable"
                    help="Both ingress and egress enable in a range of interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_RANGE_IFACE;=${ifacename},&SFLW_RANGE_INTF_DIRECTION;,&SFLW_RANGE_INTF_ENABLE;=false"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
    </VIEW>

    <!--=======================================================-->
    <!--                Config LAG range MODE            -->
    <!--=======================================================-->
    <VIEW
        name="configure-lag-range-view"
        >
        <!-- sflow [direction-] enable -->
        <COMMAND
            name="sflow"
            help="Sflow configurations in a range of lag interfaces"
            operation="edit-config"
            namespace="&SFLW_INTF_NS;"
            >
            <PARAM
                name="sampling-dir-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="enable"
                    help="Enables ingress sflow in a range of lag interfaces"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_RANGE_IFACE;=${portchanliface},&SFLW_RANGE_INTF_ENABLE;=true,&SFLW_RANGE_INTF_DIRECTION;=ingress"
                    />
                <PARAM
                    name="egress-enable"
                    help="Egress enable in a range of lag interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_RANGE_IFACE;=${portchanliface},&SFLW_RANGE_INTF_ENABLE;=true,&SFLW_RANGE_INTF_DIRECTION;=egress"
                    />
                <PARAM
                    name="both-enable"
                    help="Both ingress and egress enable in a range of lag interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&SFLW_RANGE_IFACE;=${portchanliface},&SFLW_RANGE_INTF_ENABLE;=true,&SFLW_RANGE_INTF_DIRECTION;=both"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
        <!-- no sflow [direction-] enable -->
        <COMMAND
            name="no sflow"
            operation="edit-config"
            namespace="&SFLW_INTF_NS;"
            help="Deletes the sFlow configurations for a range of lag interfaces"
            >
            <PARAM
                name="nosampling-dir-subcmds"
                help="Sub-commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="enable"
                    help="Disables ingress sflow for a range of lag interfaces"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_RANGE_IFACE;=${portchanliface},&SFLW_RANGE_INTF_DIRECTION;,&SFLW_RANGE_INTF_ENABLE;=false"
                    />
                <PARAM
                    name="egress-enable"
                    help="Egress disable in a range of lag interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_RANGE_IFACE;=${portchanliface},&SFLW_RANGE_INTF_DIRECTION;,&SFLW_RANGE_INTF_ENABLE;=false"
                    />
                <PARAM
                    name="both-enable"
                    help="Both ingress and egress enable in a range of lag interfaces"
                    pfeature="NOT_SUPPORTED"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_RANGE_IFACE;=${portchanliface},&SFLW_RANGE_INTF_DIRECTION;,&SFLW_RANGE_INTF_ENABLE;=false"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
