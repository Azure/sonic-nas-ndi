                    sub_oper_yang="remove:direction,remove:enable"
                    yang_name="&SFLW_IFACE;=${port-channeliface},&SFLW_INTF_DIRECTION;,&SFLW_INTF_ENABLE;=false"
                    />
                <PARAM
                    name="both-enable"
                    help="Both ingress and egress disable in lag interface"
