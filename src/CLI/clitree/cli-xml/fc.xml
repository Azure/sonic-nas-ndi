<!DOCTYPE CLISH_MODULE [
<!ENTITY SUBTREE_FILTER "Filter_Type:SUBTREE;">
<!ENTITY NAME_SPACE     "http://www.dellemc.com/networking/os10/dell-fc-services">
<!ENTITY NS_FILTER      "Filter_Namespace:dell-fc-services=&NAME_SPACE;">
<!ENTITY FC_CFG         "/fc-cfg">
<!ENTITY FC-PREF        "dell-fc-services">
<!ENTITY FC_ALIAS       "&FC_CFG;/alias">
<!ENTITY FC_ALIAS_KEY   "&FC_ALIAS;/name">
<!ENTITY FC_ALIAS_LIST  "&FC_ALIAS;[]">
<!ENTITY FC_ALIAS_NAME  "&FC_ALIAS_LIST;/name">
<!ENTITY FC_ZONE        "&FC_CFG;/zone">
<!ENTITY FC_ZONE_KEY    "&FC_ZONE;/name">
<!ENTITY FC_ZONE_LIST   "&FC_ZONE;[]">
<!ENTITY FC_ZONE_NAME   "&FC_ZONE_LIST;/name">
<!ENTITY FC_ZONESET     "&FC_CFG;/zoneset[]">
<!ENTITY FC_ZONESET_KEY "&FC_CFG;/zoneset/name">
<!ENTITY FC_ZONESET_ZONE_LIST   "&FC_CFG;/zoneset/zone-name[]">
<!ENTITY FC_OPER_PARAMS "/fc-oper-params">
<!ENTITY VFABRIC_CFG    "&FC_CFG;/vfabric[]">
<!ENTITY SHOW_VFAB_CFG    "&FC_CFG;/vfabric">
<!ENTITY VFABRIC_CFG_KEY    "&FC_CFG;/vfabric/vfabric-id">
<!ENTITY VFABRIC_CFG_VLAN_LIST  "&FC_CFG;/vfabric/vlan-id[]">
<!ENTITY VFABRIC_OPER   "&FC_OPER_PARAMS;/vfabric-db">
<!ENTITY FC_NS_PREF     "&FC-PREF;=http://www.dellemc.com/networking/os10/dell-fc-services">
<!ENTITY FC_INTF_NS     "urn:ietf:params:xml:ns:yang:ietf-interfaces,&FC_NS_PREF;">
<!ENTITY INTF_CONFIG    "/interfaces/interface">
<!ENTITY FC_INTF_NAME   "&INTF_CONFIG;/name=${ifacename}">
<!ENTITY FC_INTF_CONFIG "&INTF_CONFIG;/dell-fc-services:fc-params">
<!ENTITY FC_RANGE_IF_ENTRY  "/interfaces/interface[]">
<!ENTITY FC_RANGE_IF_NAME   "&FC_RANGE_IF_ENTRY;/name=${ifacename}">
<!ENTITY FC_RANGE_IF_CONFIG "&FC_RANGE_IF_ENTRY;/dell-fc-services:fc-params">
<!ENTITY FC_OPER_PARAMS_VFABRIC "/fc-oper-params/vfabric-db">
<!ENTITY FC_OPER_PARAMS_FC_INTF "/fc-oper-params/fc-intf-db">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
            http://www.dellemc.com/force10/XMLSchema/clish.xsd">
<!-- ********************************************************************** -->
<!-- *  FC COMMANDS                                                       * -->
<!--
[no] fc alias <alias-name>
	[no] member (fcid/wwn/member-port) <fcid-val/wwn-val/member-port-val>
[no] fc zone <zone-name>
	[no] member (fc-alias/fc-id/wwn/member-port) <alias-name/fcid-val/wwn-val/member-port-val>
[no] fc zoneset <zoneset-name>
    [no] member zone <zone-name>
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->
    <!-- Config Mode -->
    <VIEW name="configure-view">
        <!--feature fc domain-id-->
        <COMMAND
            name="feature fc"
            help="Enable FC feature with fport functionality"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            >
            <PARAM
                name="domain-id"
                help="FPort Domain ID (1-239) (default: 1)"
                ptype="SUBCOMMAND"
                mode="subcommand"
                >
                <PARAM
                    name="domain-id-value"
                    help="FPort Domain ID (1-239) (default: 1)"
                    ptype="RANGE_1_239"
                    yang_name="&FC_CFG;/domain-id=${domain-id-value}"
                />
            </PARAM>
            <ACTION  builtin="clish_extn" > </ACTION>
        </COMMAND>

        <COMMAND
            name="fc"
            help="Configure FC"
            operation="edit-config"
            pfeature="FEATURE_ZONING"
            namespace="&NAME_SPACE;"
        >
            <PARAM
                name="fc-cfg-commands"
                help="FC Configuration Commands"
                ptype="SUBCOMMAND"
                mode="switch"
            >
                <MACRO name="FC-NAME" arg="alias"></MACRO>
                <MACRO name="FC-NAME" arg="zone"></MACRO>
                <PARAM
                    name="zoneset"
                    help="Zoneset commands"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    viewid="zoneset_name=${zonesetname}"
                    view="configure-fc-zoneset-view"
                    yang_key="&FC_ZONESET_KEY;"
                >
                    <PARAM
                        name="zonesetname"
                        help="Zoneset Name (must start with a letter and may contain characters A-Z, a-z, 0-9, $, _, -, ^)"
                        ptype="STRING"
                        yang_name="&FC_ZONESET;/name=${zonesetname}"
                    />
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <!-- vfabric <vfabric-id> -->
        <COMMAND
            name="vfabric"
            help="Configure vfabric"
            operation="edit-config"
            pfeature="FEATURE_FC"
            namespace="&NAME_SPACE;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            view="configure-fc-vfabric-view"
            viewid="fabric-id=${vfabric_id}"
            yang_key="&VFABRIC_CFG_KEY;"
        >
            <PARAM
                name="vfabric_id"
                help="vfabric id"
                ptype="RANGE_1_255"
                yang_name="&VFABRIC_CFG;/vfabric-id=${vfabric_id}"
            />
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <!-- no feature fc -->
        <COMMAND
            name="no feature fc"
            help="Disable FC Feature"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            yang_name="&FC_CFG;/domain-id"
            sub_oper_yang="delete:domain-id"
            >
            <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>

        <COMMAND
            name="no fc"
            help="Unconfigure FC"
            ptype="SUBCOMMAND"
            operation="edit-config"
            pfeature="FEATURE_ZONING"
            namespace="&NAME_SPACE;"
        >
            <PARAM
                name="no-fc-cfg"
                help="Unconfigure FC"
                ptype="SUBCOMMAND"
                mode="switch"
            >
                <MACRO name="NO-FC-NAME" arg="alias"></MACRO>
                <MACRO name="NO-FC-NAME" arg="zone"></MACRO>
                <PARAM
                    name="zoneset"
                    help="Remove zoneset commands"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_key="&FC_ZONESET_KEY;"
                >
                    <PARAM
                        name="zonesetname"
                        help="Zoneset Name (must start with a letter and may contain characters A-Z, a-z, 0-9, $, _, -, ^)"
                        ptype="STRING"
                        sub_oper_yang="delete:zoneset"
                        yang_name="&FC_ZONESET;/name=${zonesetname}"
                    />
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="no vfabric"
            help="Disable vfabric"
            operation="edit-config"
            pfeature="FEATURE_FC"
            namespace="&NAME_SPACE;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            yang_key="&VFABRIC_CFG_KEY;"
        >
            <PARAM
                name="vfabric_id"
                help="vfabric id"
                ptype="RANGE_1_255"
                yang_name="&VFABRIC_CFG;/vfabric-id=${vfabric_id}"
                sub_oper_yang="remove:vfabric"
            />
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
    </VIEW>

    <VIEW
        name="configure-fc-alias-view"
        prompt="${SYSTEM_NAME}(config-alias-${name})# "
        depth="2"
    >
        <!-- Inheritance -->
        <NAMESPACE
            ref="configure-view"
            help="false"
            completion="false"
        />

        <COMMAND
            name="member"
            help="FC alias member"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            yang_name="&FC_ALIAS_NAME;=${name}"
        >
            <MACRO name="FC-MEMBER-SWITCH" arg="alias"></MACRO>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="no member"
            help="Unconfigure FC alias member"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            yang_name="&FC_ALIAS_NAME;=${name}"
        >
            <MACRO name="FC-NO-MEMBER-SWITCH" arg="alias"></MACRO>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="show"
            help="show configuration"
        />
        <COMMAND
            name="show configuration"
            help="Current configuration (default = running)"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ALIAS_NAME;=${name};&NS_FILTER;"
        >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="show configuration candidate"
            help="Current candidate configuration"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ALIAS_NAME;=${name};&NS_FILTER;"
        >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
    </VIEW>


    <VIEW
        name="configure-fc-zone-view"
        prompt="${SYSTEM_NAME}(config-zone-${name})# "
        depth="2"
    >
        <!-- Inheritance -->
        <NAMESPACE ref="configure-view"
            help="false"
            completion="false"
        />

        <COMMAND
            name="member"
            help="FC zone member"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            yang_name="&FC_ZONE_NAME;=${name}"
        >
            <PARAM
                name="fc-zone-member-options"
                help=""
                ptype="SUBCOMMAND"
                mode="switch"
            >
                <MACRO name="FC-MEMBER-OPTIONS" arg="zone"></MACRO>
                <PARAM
                    name="alias-name"
                    help="FC zone member alias name"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                >
                    <PARAM
                        name="aname"
                        help="FC zone member alias name"
                        ptype="STRING_32"
                        yang_name="&FC_ZONE_LIST;/alias-name=${aname}"
                    />
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="no member"
            help="Unconfigure FC zone member"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            yang_name="&FC_ZONE_NAME;=${name}"
        >
            <PARAM
                name="fc-no-zone-member-options"
                help="Unconfigure FC zone member"
                ptype="SUBCOMMAND"
                mode="switch"
            >
                <MACRO name="FC-NO-MEMBER-OPTIONS" arg="zone"></MACRO>
                <PARAM
                    name="alias-name"
                    help="Unconfigure FC zone member alias name"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    sub_oper_yang="delete:alias-name"
                >
                    <PARAM
                        name="aname"
                        help="FC zone member alias name"
                        ptype="STRING_32"
                        yang_name="&FC_ZONE_LIST;/alias-name=${aname}"
                    />
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="show"
            help="show configuration"
        />
        <COMMAND
            name="show configuration"
            help="Current configuration (default = running)"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ZONE_NAME;=${name};&NS_FILTER;"
        >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="show configuration candidate"
            help="Current candidate configuration"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ZONE_NAME;=${name};&NS_FILTER;"
        >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
    </VIEW>

    <!--=======================================================-->
    <!--                FC Zoneset MODE                        -->
    <!--=======================================================-->
    <VIEW
        name="configure-fc-zoneset-view"
        prompt="${SYSTEM_NAME}(conf-fc-zoneset-${zoneset_name})# "
        depth="2"
        >
        <!-- Inheritance -->
        <NAMESPACE
            ref="configure-view"
            help="false"
            completion="false"
            />
        <!-- member -->
        <COMMAND
            name="member"
            operation="edit-config"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
            help="Zone name to be added as member of the zoneset"
            >
            <PARAM
                name="zonename"
                help="Zone name"
                ptype="STRING"
                yang_name="&FC_ZONESET;/name=${zoneset_name},&FC_ZONESET_ZONE_LIST;=${zonename}"
                />
            <ACTION
                builtin="clish_extn"
            ></ACTION>
        </COMMAND>
        <!-- no member -->
        <COMMAND
            name="no"
            help="Negate a command"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            >
            <PARAM
                name="member"
                help="Remove a zone as a member from zoneset"
                ptype="SUBCOMMAND"
                mode="subcommand"
                >
                <PARAM
                    name="zonename"
                    help="Zone name"
                    ptype="STRING"
                    sub_oper_yang="remove:zone-name"
                    yang_name="&FC_ZONESET;/name=${zoneset_name},&FC_ZONESET_ZONE_LIST;=${zonename}"
                    />
            </PARAM>
            <ACTION
                builtin="clish_extn"
            ></ACTION>
        </COMMAND>

        <COMMAND
            name="show"
            help="show configuration"
        />
        <COMMAND
            name="show configuration"
            help="Current configuration (default = running)"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ZONESET;/name=${zoneset_name};&NS_FILTER;"
        >
        <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="show configuration candidate"
            help="Current candidate configuration"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ZONESET;/name=${zoneset_name};&NS_FILTER;"
        >
        <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
    </VIEW>

    <VIEW
        name="configure-fc-vfabric-view"
        prompt="${SYSTEM_NAME}(conf-vfabric-${fabric-id})# "
        depth="2"
    >
        <!-- Inheritance -->
        <NAMESPACE
            ref="configure-view"
            help="false"
            completion="false"
        />
        <!-- name <name> -->
        <COMMAND
            name="name"
            help="Configure the vfabric name"
            operation="edit-config"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
        >
            <PARAM
                name="vfabric_name"
                help="vfabric name"
                ptype="STRING"
                yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/description=${vfabric_name}"
            />
            <ACTION builtin="clish_extn" ></ACTION>
        </COMMAND>
        <!-- vlan <vlan-id> -->
        <COMMAND
            name="vlan"
            help="Configure the vlan id for the vfabric"
            operation="edit-config"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
        >
            <PARAM
                name="vlan_id"
                help="vlan id"
                ptype="RANGE_1_4095"
                yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG_VLAN_LIST;=vlan${vlan_id}"
                template="fc_svcs_err.xsl"
            />
            <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
        <!-- fcoe fcmap <0EFC00-0EFCFF>-->
        <COMMAND
            name="fcoe"
            help="Configure fcoe parameters for the vfabric"
            operation="edit-config"
            pfeature="FEATURE_FC"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
        >
            <PARAM
                name="fcmap"
                help="Configure the fcoe fcmap for the vfabric"
                ptype="SUBCOMMAND"
                mode="subcommand"
            >
                <PARAM
                    name="fc_map"
                    help="fcmap"
                    ptype="HEX_RANGE_EFC00_EFCFF"
                    yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/fc-map=${fc_map}"
                />
            </PARAM>
            <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
        <!-- zoneset activate name <zoneset-name> -->
        <COMMAND
            name="zoneset"
            help="Configue the zoneset name to activate"
            operation="edit-config"
            ptype="SUBCOMMAND"
            pfeature="FEATURE_ZONING"
            mode="subcommand"
            namespace="&NAME_SPACE;"
        >
            <PARAM
                name="activate"
                help="Configue the zoneset name to activate"
                ptype="SUBCOMMAND"
                mode="subcommand"
            >
                <PARAM
                   name="zonesetname"
                   help="Configure zoneset name"
                   ptype="STRING"
                   yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/active-zoneset=${zonesetname}"
                />
            </PARAM>
            <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
        <!-- zone default-zone permit -->
        <COMMAND
            name="zone"
            help="Configure allow or deny default zone"
            operation="edit-config"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
        >
            <PARAM
                name="default-zone"
                help="Configure the default zone"
                ptype="SUBCOMMAND"
                mode="subcommand"
            >
                <PARAM
                    name="permit"
                    help="Permit the default zone"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/default-zone-allow=true"
                />
            </PARAM>
            <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
        <COMMAND
            name="no"
            help="Disable the vfabric related configuration"
            operation="edit-config"
            ptype="SUBCOMMAND"
            mode="subcommand"
            namespace="&NAME_SPACE;"
        >
                <PARAM
                    name="all no commands"
                    help=""
                    ptype="SUBCOMMAND"
                    mode="switch"
                >
                    <!-- no name <vfabric-name -->
                    <PARAM
                        name="name"
                        help="Configure the vfabric name"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        namespace="&NAME_SPACE;"
                        yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/description"
                        sub_oper_yang="remove:description"
                    />
                    <!-- no vlan <vlan-id> -->
                    <PARAM
                        name="vlan"
                        help="Configure the vlan id for the vfabric"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        namespace="&NAME_SPACE;"
                    >
                        <PARAM
                            name="vlan_id"
                            help="vlan id"
                            ptype="RANGE_1_4095"
                            yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG_VLAN_LIST;=vlan${vlan_id}"
                            sub_oper_yang="remove:vlan-id"
                        />
                    </PARAM>
                    <!-- no fcoe fcmap -->
                    <PARAM
                        name="fcoe"
                        help="Configure the default fcoe parameters for the vfabric"
                        pfeature="FEATURE_FC"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        namespace="&NAME_SPACE;"
                    >
                        <PARAM
                            name="fcmap"
                            help="Configure the default fcmap for the vfabric"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            namespace="&NAME_SPACE;"
                            yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/fc-map"
                            sub_oper_yang="remove:fc-map"
                        />
                    </PARAM>
                    <!-- no zone default-zone permit -->
                    <PARAM
                        name="zone"
                        help="Configure allow or deny default zone"
                        operation="edit-config"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        namespace="&NAME_SPACE;"
                    >
                        <PARAM
                            name="default-zone"
                            help="Configure the default zone"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                        >
                            <PARAM
                                name="permit"
                                help="Permit the default zone"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/default-zone-allow=false"
                            />
                        </PARAM>
                    </PARAM>
                    <PARAM
                        name="zoneset"
                        help="Configue the zoneset name to activate"
                        operation="edit-config"
                        pfeature="FEATURE_ZONING"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        namespace="&NAME_SPACE;"
                    >
                        <PARAM
                            name="activate"
                            help="Configue the zoneset name to activate"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&VFABRIC_CFG;/vfabric-id=${fabric-id},&VFABRIC_CFG;/active-zoneset"
                            sub_oper_yang="remove:active-zoneset"
                        />
                    </PARAM>
                </PARAM>
                <ACTION builtin="clish_extn"></ACTION>
            </COMMAND>

            <COMMAND
                name="show"
                help="show configuration"
            />
            <COMMAND
                name="show configuration"
                help="Current configuration (default = running)"
                yang_filter="&SUBTREE_FILTER;Filter_Conditions:&VFABRIC_CFG;/vfabric-id=${fabric-id};&NS_FILTER;"
            >
                <ACTION builtin="clish_extn"> </ACTION>
            </COMMAND>
            <COMMAND
                name="show configuration candidate"
                help="Current candidate configuration"
                yang_filter="&SUBTREE_FILTER;Filter_Conditions:&VFABRIC_CFG;/vfabric-id=${fabric-id};&NS_FILTER;"
            >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
    </VIEW>

    <!--=======================================================-->
    <!--              FC INTERFACE MODE config                  -->
    <!--=======================================================-->
    <VIEW name="configure-fc-view">
        <COMMAND
            name="vfabric"
            help="Configure vfabric on interface"
            operation="edit-config"
            pfeature="FEATURE_FC"
            namespace="&FC_INTF_NS;"
            ptype="SUBCOMMAND"
            mode="subcommand"
        >
            <PARAM
                name="vfabric_id"
                help="vfabric id"
                ptype="RANGE_1_255"
                yang_name="&FC_INTF_NAME;,&FC_INTF_CONFIG;/dell-fc-services:vfabric-id=${vfabric_id}">
            </PARAM>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="no vfabric"
            help="Remove/delete vfabric on interface"
            operation="edit-config"
            pfeature="FEATURE_FC"
            namespace="&FC_INTF_NS;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            yang_name="&FC_INTF_NAME;,&FC_INTF_CONFIG;/dell-fc-services:vfabric-id"
            sub_oper_yang="remove:vfabric-id"
        >
        <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
    </VIEW>

    <!--=======================================================-->
    <!--               FC INTERFACE RANGE MODE config           -->
    <!--=======================================================-->
    <VIEW name="configure-fc-range-view">
        <COMMAND
            name="vfabric"
            help="Configure vfabric on interface"
            operation="edit-config"
            pfeature="FEATURE_FC"
            namespace="&FC_INTF_NS;"
            ptype="SUBCOMMAND"
            mode="subcommand"
        >
            <PARAM
                name="vfabric_id"
                help="vfabric id"
                ptype="RANGE_1_255"
                yang_name="&FC_RANGE_IF_NAME;,&FC_RANGE_IF_CONFIG;/dell-fc-services:vfabric-id=${vfabric_id}"
            >
            </PARAM>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="no vfabric"
            help="Remove/delete vfabric on interface"
            operation="edit-config"
            pfeature="FEATURE_FC"
            namespace="&FC_INTF_NS;"
            ptype="SUBCOMMAND"
            mode="subcommand"
            yang_name="&FC_RANGE_IF_NAME;,&FC_RANGE_IF_CONFIG;/dell-fc-services:vfabric-id"
            sub_oper_yang="remove:vfabric-id"
        >
        <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
    </VIEW>

    <!-- Exec Mode -->
    <VIEW name="enable-view">
        <COMMAND
            name="show running-configuration fc"
            help="Current operating FC configuration"
            pfeature="FEATURE_FC"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_CFG;;&NS_FILTER;"
            db_mode="configure,configure-fc-alias,configure-fc-zone"
        >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="show candidate-configuration fc"
            help="Current candidate FC configuration"
            pfeature="FEATURE_FC"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_CFG;;&NS_FILTER;"
            db_mode="configure,configure-fc-alias,configure-fc-zone"
        >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <COMMAND
            name="show vfabric"
            help="Show the vfabric configurations"
            operation="get"
            namespace="&NS_FILTER;"
            pfeature="FEATURE_FC"
            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_OPER_PARAMS;;&NS_FILTER;|&SUBTREE_FILTER;Filter_Conditions:&FC_INTF_CONFIG;;Filter_Namespace:&FC_INTF_NS;|&SUBTREE_FILTER;Filter_Conditions:&FC_CFG;;&NS_FILTER;"
            template="show_vfabric.xsl">
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="show fc"
            help="Show FC"
            operation="get"
            pfeature="FEATURE_FC"
            namespace="&NS_FILTER;"
        >
            <PARAM
                name="show-fc-commands"
                help="Show FC Commands"
                ptype="SUBCOMMAND"
                mode="switch"
            >
                <PARAM
                    name="statistics"
                    help="Show FC Switch parameters"
                    ptype="SUBCOMMAND"
                    mode="subcommand">
                    <PARAM
                        name="fc statistics options"
                        help="fc statistics options"
                        ptype="SUBCOMMAND"
                        mode="switch">
                        <PARAM
                            name="vfabric"
                            help="Show fc statistics vfabric"
                            ptype="SUBCOMMAND"
                            mode="subcommand">
                            <PARAM
                                name="vfabric_id"
                                help="vfabric id"
                                ptype="RANGE_1_255"
                                template="show_fc_vfab_statistics.xsl"
                                yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_OPER_PARAMS;/vfabric-db,&FC_OPER_PARAMS;/vfabric-db/vfabric-id=${vfabric_id};Filter_Namespace:&NAME_SPACE;"/>
                        </PARAM> <!-- vfabric -->
                        <PARAM
                            name="fiberChannel"
                            help="Show fc statistics fiberChannel"
                            ptype="SUBCOMMAND"
                            mode="subcommand">
                            <PARAM
                                name="fc-port"
                                help="FC port"
                                ptype="FC_INTERFACE"
                                yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_OPER_PARAMS_FC_INTF;/fc-if=${fc-port};&NS_FILTER;"/>
                                <!-- template="show_fc_port_statistics.xsl" -->
                        </PARAM> <!-- fiberChannel -->
                    </PARAM> <!-- fc statistics options -->
                </PARAM> <!-- statistics -->
                <!--  show fc switch  -->
                <PARAM
                    name="switch"
                    help="Show FC Switch parameters"
                    template="show_fc.xsl"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_filter="&SUBTREE_FILTER;Filter_Conditions:/fc-oper-params;&NS_FILTER;"
                />
                <!-- show fc ns switch -->
                <PARAM
                    name="ns"
                    help="Show FC NS Switch parameters"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                >
                    <PARAM
                        name="switch"
                        help="Show FC NS Switch parameters"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_filter="&SUBTREE_FILTER;Filter_Conditions:/fc-oper-params;&NS_FILTER;"
                        template="show_fc_ns_switch.xsl"
                    >
                        <PARAM
                            name="brief"
                            help="Show FC NS Switch parameters"
                            optional="true"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_filter="&SUBTREE_FILTER;Filter_Conditions:/fc-oper-params;&NS_FILTER;"
                            template="show_fc_ns_switch_brief.xsl"
                        />
                    </PARAM>
                </PARAM>
                <!--  show fc alias  -->
                <PARAM
                    name="alias"
                    help="Show FC alias"
                    pfeature="FEATURE_ZONING"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ALIAS;;&NS_FILTER;"
                    template="show_fc_alias.xsl"
                >
                    <PARAM
                        name="name"
                        help="Show FC alias"
                        optional="true"
                        ptype="STRING_32"
                        yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ALIAS_NAME;=${name};&NS_FILTER;"
                        template="show_fc_alias.xsl"
                    />
                </PARAM>
                <!--  show fc zone  -->
                <PARAM
                    name="zone"
                    help="Show FC Zone"
                    pfeature="FEATURE_ZONING"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ZONE;;&NS_FILTER;"
                    template="show_fc_zone.xsl"
                >
                    <PARAM
                        name="name"
                        help="Show FC Zone"
                        optional="true"
                        ptype="STRING_32"
                        yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_ZONE_NAME;=${name};&NS_FILTER;"
                        template="show_fc_zone.xsl"
                    />
                </PARAM>
                <PARAM
                    name="zoneset"
                    help="Show fc zoneset"
                    pfeature="FEATURE_ZONING"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    template="show_fc_zoneset.xsl"
                    yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_CFG;;&NS_FILTER;">
                    <PARAM
                        name="show fc zoneset commands"
                        help="show fc zoneset commands"
                        ptype="SUBCOMMAND"
                        optional="true"
                        mode="switch">
                        <PARAM
                            name="active"
                            help="Show fc zoneset active"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            template="show_fc_zoneset_active.xsl"
                            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_CFG;;&NS_FILTER;"
                        />  <!-- show fc zoneset active-->
                        <PARAM
                            name="zonesname"
                            help="Show fc zoneset (zoneset name or list)"
                            ptype="STRING"
                            template="show_fc_zoneset.xsl"
                            yang_filter="&SUBTREE_FILTER;Filter_Conditions:&FC_CFG;,&FC_ZONESET;/name=${zonesname};&NS_FILTER;"
                        />  <!-- show fc zoneset -->
                    </PARAM> <!-- show fc zoneset -->
                </PARAM> <!-- show fc switch -->
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>
</CLISH_MODULE>
