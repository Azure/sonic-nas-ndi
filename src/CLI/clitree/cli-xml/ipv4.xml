<!ENTITY MODULE-INTF-IP-NS     "&MODULE-NS-IETF-INTF;,&MODULE-PREF-DELL-IP;=&MODULE-NS-DELL-IP;">
<!ENTITY GLOBAL_IPV4           "/ip-config/ipv4">
<!ENTITY GLOBAL_ARP            "&GLOBAL_IPV4;/arp">
<!ENTITY ARP_OPER	           "/ip-oper/ipv4/arp-oper">
<!ENTITY ARP_OPER_ENTRY        "/ip-oper/ipv4/arp-oper/arp-entry">
<!ENTITY INTF_ENTRY            "/interfaces/interface">
<!ENTITY INTF_NAME             "&INTF_ENTRY;/name=${ifacename}">
<!ENTITY PO_NAME               "&INTF_ENTRY;/name=${port-channeliface}">
<!ENTITY VL_NAME               "&INTF_ENTRY;/name=${vlan_name}">
<!ENTITY MGMT_NAME             "&INTF_ENTRY;/name=${ifacename}">
<!ENTITY LOOPBACK_NAME         "&INTF_ENTRY;/name=${ifacename}">
<!ENTITY INTF_IPV4             "&INTF_ENTRY;/&MODULE-PREF-DELL-IP;:ipv4">
<!ENTITY INTERFACE_NAME        "/interfaces-state/interface">
<!ENTITY IP_ARP_COUNT          "&INTERFACE_NAME;/dell-ip:l3-arp-params">
<!ENTITY INTF_ARP              "&INTF_IPV4;/&MODULE-PREF-DELL-IP;:arp">
<!ENTITY INTF_ARP_ENTRY        "&INTF_ARP;/&MODULE-PREF-DELL-IP;:arp-entry">
<!ENTITY INTF_ARP_ADDR         "&INTF_ARP_ENTRY;/&MODULE-PREF-DELL-IP;:addr">
<!ENTITY INTF_IPV4_ADDR_DHCP   "&INTF_IPV4;/&MODULE-PREF-DELL-IP;:dhcp-config/&MODULE-PREF-DELL-IP;:is-dhcp">
