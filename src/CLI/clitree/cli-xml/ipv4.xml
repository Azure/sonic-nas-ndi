<!ENTITY IP_RANGE_INTF "/interfaces/interface[]">
<!ENTITY IP_RANGE_IFACE "&IP_RANGE_INTF;/name">
<!ENTITY IP_RANGE_INTF_INFO "&IP_RANGE_INTF;/&MODULE-PREF-DELL-IP;:ipv4">
]>


<CLISH_MODULE xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema http://www.dellemc.com/force10/XMLSchema/clish.xsd">

<VIEW name="enable-view">
    <COMMAND
            name="clear ip"
            help="Clear ip command">
    </COMMAND>
    <COMMAND
            name="debug ip"
            pfeature="NOT_SUPPORTED"
            help="Debug ip command">
    </COMMAND>
    <COMMAND
        help="show IP commands"
        name="show ip"
        namespace="&MODULE-NS-DELL-IP;"
        operation="get"
        >
    <ACTION builtin="clish_nop"/>
    </COMMAND>
</VIEW>

<!--=======================================================-->
<!--                Config Interface PHY-MODE              -->
<!--=======================================================-->

<VIEW name="configure-if-view">
    <COMMAND
        name="ip"
        help="Interface Internet Protocol config commands"
    />
    <COMMAND
        name="no ip"
        help="Interface Internet Protocol config commands"
    />
  </VIEW>

<!--=======================================================-->
<!--                Config Interface LAG-MODE              -->
<!--=======================================================-->

<VIEW name="configure-lag-view">
    <COMMAND
        name="ip"
        help="Interface Internet Protocol config commands"
    />
    <COMMAND
        name="no ip"
        help="Interface Internet Protocol config commands"
    />
    <!--- ip unreachables -->
    <COMMAND
        name="ip unreachables"
        help="Enable IP unreachables"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        yang_name= "&PO_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:unreachables=true"
        pfeature="NOT_SUPPORTED"
        >
        <ACTION builtin="clish_nop" ></ACTION>
    </COMMAND>
    <!--- ip redirects -->
    <COMMAND
        name="ip redirects"
        help="Enable ICMP redirect messages"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        yang_name= "&PO_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:redirects=true"
        pfeature="NOT_SUPPORTED"
        >
        <ACTION builtin="clish_nop" ></ACTION>
    </COMMAND>

    <!--- no ip unreachables -->
    <COMMAND
        name="no ip unreachables"
        help="Enable IP unreachables"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        sub_oper_yang="remove:unreachables"
        yang_name= "&PO_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:unreachables"
        pfeature="NOT_SUPPORTED"
        >
        <ACTION builtin="clish_nop" ></ACTION>
    </COMMAND>
    <!--- no ip redirects -->
    <COMMAND
        name="no ip redirects"
        help="Enable ICMP redirect messages"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        sub_oper_yang="remove:redirects"
        yang_name= "&PO_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:redirects"
        pfeature="NOT_SUPPORTED"
        >
        <ACTION builtin="clish_nop" ></ACTION>
    </COMMAND>
    <!-- IP address -->
     <COMMAND
        name="ip address dhcp"
        help="Enables dhcp client operations on an interface"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        yang_name="&PO_NAME;,&INTF_IPV4_ADDR_DHCP;=true">
        <ACTION builtin="clish_nop"></ACTION>
     </COMMAND>

     <!-- IP address -->
     <COMMAND
        name="ip address"
        help="Set the IP address of an interface"
        namespace="&MODULE-INTF-IP-NS;"
        db_multi_yang_name="&INTF_IPV4_PRI_ADDR;,&INTF_IPV4_SEC_ADDR;"
        db_multi_yang_key="-||&INTF_IPV4_SEC_ADDR;;;leaf-list"
        db_flag="SEP_CLI"
        operation="edit-config"
        >
    <!--- ip address <ip-address/mask> [secondary]  -->
        <PARAM
            help="Set IP address"
            name="addr"
            ptype="IP_ADDR_MASK"
            yang_name= "&PO_NAME;,&INTF_IPV4_PRI_ADDR;=${addr}"
            namespace="&MODULE-INTF-IP-NS;">
            <PARAM
                name="addr-type"
                help="IP address type"
                optional="true"
                ptype="SUBCOMMAND"
                db_flag="SEP_CLI"
                mode="switch">
                <PARAM
                    test='"${addr}" = "hide_primary"'
                    help="Make this IP address a primary address"
                    name="primary"
                    db_flag="SKIP_NODE"
                    mode="subcommand"
                    ptype="SUBCOMMAND"/>
                <PARAM
                    help="Make this IP address a secondary address"
                    name="secondary"
                    prompt_fn="process_sec_ip_addr"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name= "&INTF_IPV4_SEC_ADDR;=${addr}"/>
                </PARAM>
            </PARAM>
        <ACTION
            builtin="clish_nop"
            ></ACTION>
    </COMMAND>
    <COMMAND
            name="no ip address dhcp"
            help="Enables dhcp client operations on an interface"
            namespace="&MODULE-INTF-IP-NS;"
            operation="edit-config"
            sub_oper_yang="remove:dhcp-config"
            yang_name="&PO_NAME;,&INTF_IPV4_ADDR_DHCP;=false">
            <ACTION builtin="clish_nop"></ACTION>
        </COMMAND>
    <COMMAND
        name="no ip address"
        help="Set the IP address of an interface"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        sub_oper_yang="remove:address,remove:dhcp-config"
        yang_name= "&PO_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:address,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:dhcp-config"
        >
        <!--- no ip address <ip-address/mask> [secondary]   -->
        <PARAM
            help="Set IP address"
            name="addr"
            optional="true"
            ptype="IP_ADDR_MASK"
            refresh_yang_name="true"
            sub_oper_yang="clear:remove:address"
            yang_name= "&PO_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:address/&MODULE-PREF-DELL-IP;:primary-addr=${addr}">
            <PARAM
                name="addr-type"
                help="IP address type"
                ptype="SUBCOMMAND"
                optional="true"
                mode="switch">
                <PARAM
                    test='"${addr}" = "hide_primary"'
                    help="Make this IP address a primary address"
                    name="primary"
                    mode="subcommand"
                    db_flag="SKIP_NODE"
                    ptype="SUBCOMMAND"/>
                <PARAM
                    help="Make this IP address a secondary address"
                    name="secondary"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    sub_oper_yang="clear;remove:secondary-addr"
                    refresh_yang_name="true"
                    yang_name= "&PO_NAME;,&INTF_IPV4_SEC_ADDR;=${addr}"/>
            </PARAM>
        </PARAM>
        <ACTION
            builtin="clish_nop"
            ></ACTION>
    </COMMAND>
    <!--- ip arp subcommands   -->
    <COMMAND
        help="ARP configuration"
        name="ip arp"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config">
             <PARAM
                 help="switch subcommand"
                 name="type"
                 mode="switch"
                 ptype="SUBCOMMAND">
                     <!--- ip arp <ip-address> <mac-address>   -->
                     <PARAM
                         help="Set IP address of the ARP entry"
                         name="arp-addr"
                         ptype="IP_ADDR"
                         yang_name= "&PO_NAME;,&INTF_ARP_ADDR;=${arp-addr}"
                         yang_key = "&INTF_ARP_ADDR;">
                         <PARAM
                             help="Set hardware address of ARP entry"
                             name="mac-addr"
                             ptype="MAC_ADDR"
                             yang_name= "&INTF_ARP_ENTRY;/&MODULE-PREF-DELL-IP;:link-layer-address=${mac-addr}"/>
                     </PARAM>
                     <!--- ip arp gratuitous -->
                     <PARAM
                         help="Gratuitous"
                         name="gratuitous"
                         mode="subcommand"
                         ptype="SUBCOMMAND">
                              <PARAM
                                  help="Gratuituos type subcommand"
                                  name="gratuituos-type"
                                  mode="switch"
                                  ptype="SUBCOMMAND">
                                       <!--- ip arp gratuitous update   -->
                                       <PARAM
                                           help="Enable/Disable arp cache updates for gratuitous arp"
                                           name="update"
                                           mode="subcommand"
                                           ptype="SUBCOMMAND"
                                           yang_name= "&PO_NAME;,&INTF_ARP;/&MODULE-PREF-DELL-IP;:update=true"/>
                                       <!--- ip arp gratuitous request-->
                                       <PARAM
                                           help="Enable/Disable sending gratutious arp req when duplicate address detected"
                                           name="request"
                                           mode="subcommand"
                                           ptype="SUBCOMMAND"
                                           yang_name= "&PO_NAME;,&INTF_ARP;/&MODULE-PREF-DELL-IP;:request=true"/>
                              </PARAM>
                     </PARAM>
             </PARAM> <!-- End arp-switch -->
      <ACTION builtin="clish_nop"/>
    </COMMAND>
  </VIEW>

