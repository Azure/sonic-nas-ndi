<!ENTITY INTF_IPV4_SEC_ADDR    "&INTF_IPV4;/&MODULE-PREF-DELL-IP;:address/&MODULE-PREF-DELL-IP;:secondary-addr">
<!ENTITY FILTER_NS             "Filter_Namespace:http://www.dellemc.com/networking/os10/dell-ip">
<!ENTITY L3_IP_NS              "dell-ip=http://www.dellemc.com/networking/os10/dell-ip">
