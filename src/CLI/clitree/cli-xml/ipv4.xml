     <COMMAND
        name="ip address"
        pfeature="FEATURE_MGMT_IF_IP"
        help="Set the IP address of an interface"
        namespace="&MODULE-INTF-IP-NS;"
        db_multi_yang_name="&INTF_IPV4_PRI_ADDR;,&INTF_IPV4_SEC_ADDR;"
        db_multi_yang_key="-||&INTF_IPV4_SEC_ADDR;;;leaf-list"
        db_flag="SEP_CLI"
        operation="edit-config"
        >
        <!--- ip address <ip-address/mask> [secondary]  -->
        <PARAM
            help="Set IP address"
            name="addr"
            ptype="IP_ADDR_MASK"
            yang_name= "&MGMT_NAME;,&INTF_IPV4_PRI_ADDR;=${addr}"
            namespace="&MODULE-INTF-IP-NS;">
            <PARAM
                name="addr-type"
                help="IP address type"
                optional="true"
                ptype="SUBCOMMAND"
                db_flag="SEP_CLI"
                mode="switch">
                <PARAM
                    test='"${addr}" = "hide_primary"'
                    help="Make this IP address a primary address"
                    name="primary"
                    db_flag="SKIP_NODE"
                    mode="subcommand"
                    ptype="SUBCOMMAND"/>
                <PARAM
                    help="Make this IP address a secondary address"
                    name="secondary"
                    prompt_fn="process_sec_ip_addr"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name= "&INTF_IPV4_SEC_ADDR;=${addr}"/>
            </PARAM>
        </PARAM>
        <ACTION
            builtin="clish_nop"
            ></ACTION>
    </COMMAND>
    <COMMAND
        name="no ip"
        pfeature="FEATURE_MGMT_IF_IP"
        help="Interface Internet Protocol config commands"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        />
    <COMMAND
        name="no ip address dhcp"
        pfeature="FEATURE_MGMT_IF_IP"
        help="Enables dhcp client operations on an interface"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        sub_oper_yang="remove:dhcp-config"
        yang_name="&MGMT_NAME;,&INTF_IPV4_ADDR_DHCP;=false">
        <ACTION builtin="clish_nop"></ACTION>
    </COMMAND>
    <COMMAND
        name="no ip address"
        pfeature="FEATURE_MGMT_IF_IP"
        help="Set the IP address of an interface"
        namespace="&MODULE-INTF-IP-NS;"
        operation="edit-config"
        sub_oper_yang="remove:dhcp-config,remove:address"
        yang_name= "&MGMT_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:address,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:dhcp-config"
        db_flag="SEP_CLI"
        >
            <!--- no ip address <ip-address/mask> [secondary]   -->
            <PARAM
                help="Set IP address"
                name="addr"
                optional="true"
                ptype="IP_ADDR_MASK"
                refresh_yang_name="true"
                sub_oper_yang="clear;remove:address"
                yang_name= "&MGMT_NAME;,&INTF_IPV4;/&MODULE-PREF-DELL-IP;:address/&MODULE-PREF-DELL-IP;:primary-addr=${addr}"
                >
                <PARAM
                    name="addr-type"
                    help="IP address type"
                    ptype="SUBCOMMAND"
                    optional="true"
                    mode="switch">
                    <PARAM
                        test='"${addr}" = "hide_primary"'
                        help="Make this IP address a primary address"
                        name="primary"
                        mode="subcommand"
                        db_flag="SKIP_NODE"
                        ptype="SUBCOMMAND"/>
                    <PARAM
                        help="Make this IP address a secondary address"
                        name="secondary"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        sub_oper_yang="clear;remove:secondary-addr"
                        refresh_yang_name="true"
                        yang_name= "&MGMT_NAME;,&INTF_IPV4_SEC_ADDR;=${addr}"/>
                </PARAM>
            </PARAM>
        <ACTION
            builtin="clish_nop"
            ></ACTION>
    </COMMAND>
  </VIEW>

