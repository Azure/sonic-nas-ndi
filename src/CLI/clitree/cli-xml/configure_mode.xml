<?xml version="1.0" encoding="UTF-8"?>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
						http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->
    <VIEW
        depth="1"
        name="configure-view"
        prompt="${SYSTEM_NAME}(config)# "
        restore="depth"
        >
        <COMMAND
            name="no"
            help="To delete / disable commands in config mode"
            />
        <COMMAND
            name="help"
            help="Display available commands"
            >
            <ACTION
                builtin="clish_help_command"
                />
        </COMMAND>
        <COMMAND name="exec-timeout"
                help="Set timeout (in seconds) for all CLI sessions"
            operation="edit-config"
            namespace="http://www.dellemc.com/networking/os10/dell-session"
                yang_key="/session-config/session/exec-timeout">
                <PARAM name="exec-timeout"
                help="CLI session timeout value (in seconds)"
                        yang_name="/session-config/session/line=console0,/session-config/session/exec-timeout=${exec-timeout}"
                ptype="RANGE_0_3600"/>
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND name="no exec-timeout"
                 help="Reset timeout value (to default), 3600 seconds"
                 operation="edit-config"
                 namespace="http://www.dellemc.com/networking/os10/dell-session"
                 yang_name="/session-config/session/line=console0,/session-config/session/exec-timeout=0"
                 sub_oper_yang="remove:exec-timeout">
                 <PARAM name="exec-timeout-value"
                        help="CLI session timeout value (in seconds)"
                        optional="true"
                        ptype="RANGE_0_3600"/>
            <ACTION builtin="clish_extn"> </ACTION>
         </COMMAND>
    </VIEW>
</CLISH_MODULE>

