<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE CLISH_MODULE [
]>
<CLISH_MODULE
	xmlns="http://www.dellemc.com/force10/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
	http://www.dellemc.com/force10/XMLSchema/clish.xsd"
	>
	<!--=================================================================-->
	<!-- ********************************************************************** -->
	<!-- *  Ping and Traceroute Options        			                      * -->
	<!-- ********************************************************************** -->

	<VIEW name="enable-view">
		<COMMAND name="ping" help="ping -h shows help" args="ping-option" args_help="ping options(-h  for help)">
			<ACTION >dn_ping.py ${ping-option}</ACTION>
		</COMMAND>
		<COMMAND name="ping6" help="ping6 -h shows help" args="ping-option" args_help="ping6 options(-h  for help)">
			<ACTION >dn_ping6.py ${ping-option}</ACTION>
		</COMMAND>
		<COMMAND name="traceroute" help="traceroute --help shows help" args="traceroute-option" args_help="traceroute options(--help for help)">
			<ACTION >sudo traceroute ${traceroute-option}</ACTION>
		</COMMAND>
	</VIEW>
</CLISH_MODULE>
