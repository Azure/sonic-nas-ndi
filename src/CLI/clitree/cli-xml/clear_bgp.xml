<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY SUBTREE_FILTER "Filter_Type:SUBTREE">
<!ENTITY NS_FILTER      "http://www.dellemc.com/networking/os10/dell-bgp">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
			http://www.dellemc.com/force10/XMLSchema/clish.xsd">
<!-- ********************************************************************** -->
<!-- *  CLEAR BGP COMMANDS                                                * -->
<!--
clear ip bgp <ip-address|ipv6-address|*> ipv4 unicast
clear ip bgp <ip-address|ipv6-address|*> ipv6 unicast
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->
    <VIEW name="enable-view">
        <COMMAND name="clear ip bgp"
            help="Clear BGP related information"
            ptype="SUBCOMMAND"
            operation="generic-rpc"
            namespace="&NS_FILTER;"
            >

            <PARAM name="clear-ip-bgp-options"
            help=""
            ptype="SUBCOMMAND"
            mode="switch">
                <PARAM name="ip-address-value"
                help="IPv4 address"
                ptype="IP_ADDR"
                yang_name="/clear_bgp/address=${ip-address-value},/clear_bgp/safi-index=unicast">
                    <PARAM name="ipv4-ipv6-option"
                    help=""
                    ptype="SUBCOMMAND"
                    optional="true"
                    mode="switch">
                        <PARAM name="flap-statistics"
                        help="Clear flap statistics"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        refresh_yang_name="true"
                        yang_name="/clear_flap_stats/vrf-name=default,/clear_flap_stats/peer=${ip-address-value}"/>
                        <PARAM name="ipv4"
                        help="clear ip bgp # ipv4"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                            <PARAM name="unicast"
                            help="clear ip bgp # ipv4 unicast"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            refresh_yang_name="true"
                            yang_name="/clear_bgp/address=${ip-address-value},/clear_bgp/address-family=ipv4,/clear_bgp/safi-index=unicast"/>
                        </PARAM>
                        <PARAM name="ipv6"
                        help="clear ip bgp # ipv6"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                            <PARAM name="unicast"
                            help="clear ip bgp # ipv6 unicast"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            refresh_yang_name="true"
                            yang_name="/clear_bgp/address=${ip-address-value},/clear_bgp/address-family=ipv6,/clear_bgp/safi-index=unicast"/>
                        </PARAM>
                        <PARAM name="soft"
                        help="Soft Reconfiguration"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                            <PARAM name="soft-options"
                            help="Soft reconfiguration"
                            ptype="SUBCOMMAND"
                            mode="switch">
                                <PARAM name="in"
                                help="Soft reconfigure inbound update"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                refresh_yang_name="true"
                                yang_name="/clear_bgp/address=${ip-address-value},/clear_bgp/soft-reconfig=in"/>
                                <PARAM name="out"
                                pfeature="NOT_SUPPORTED"
                                help="Soft reconfigure outbound update"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="/clear_bgp/address=${ip-address-value},/clear_bgp/soft-reconfig=out"/>
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>

                <PARAM name="ip"
                help="IPv6 address"
                ptype="IPV6_ADDR"
                yang_name="/clear_bgp/address=new_param1,/clear_bgp/safi-index=unicast"
                prompt_oper="${ip}"
                prompt_fn="show_trim_ip">
                    <PARAM name="ipv4-ipv6-option"
                    help=""
                    ptype="SUBCOMMAND"
                    optional="true"
                    mode="switch">
                        <PARAM name="flap-statistics"
                        help="Clear flap statistics"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        yang_name="/clear_flap_stats/vrf-name=default,/clear_flap_stats/peer=new_param1"
                        refresh_yang_name="true"
                        prompt_oper="${ip}"
                        prompt_fn="show_trim_ip"/>
                        <PARAM name="ipv4"
                        help="clear ip bgp # ipv4"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                            <PARAM name="unicast"
                            help="clear ip bgp # ipv4 unicast"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            refresh_yang_name="true"
                            yang_name="/clear_bgp/address=new_param1,/clear_bgp/address-family=ipv4,/clear_bgp/safi-index=unicast"
                            prompt_oper="${ip}"
                            prompt_fn="show_trim_ip"/>
                        </PARAM>
                        <PARAM name="ipv6"
                        help="clear ip bgp # ipv6"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                            <PARAM name="unicast"
                            help="clear ip bgp # ipv6 unicast"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
							refresh_yang_name="true"
							yang_name="/clear_bgp/address=new_param1,/clear_bgp/address-family=ipv6,/clear_bgp/safi-index=unicast"
							prompt_oper="${ip}"
							prompt_fn="show_trim_ip"/>
                        </PARAM>
                        <PARAM name="soft"
                        help="Soft reconfiguration"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
						    <PARAM name="soft-option"
						     help="Soft reconfiguration"
						     ptype="SUBCOMMAND"
						     mode="switch">
                                <PARAM name="in"
                                help="Soft reconfigure inbound update"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                refresh_yang_name="true"
                                yang_name="/clear_bgp/address=new_param1,/clear_bgp/soft-reconfig=in"
                                prompt_oper="${ip}"
                                prompt_fn="show_trim_ip"/>
                                <PARAM name="out"
                                pfeature="NOT_SUPPORTED"
                                help="Soft reconfigure outbound update"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="/clear_bgp/address=new_param1,/clear_bgp/soft-reconfig=out"
                                prompt_oper="${ip}"
                                prompt_fn="show_trim_ip"/>
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM name="*"
                help="clear ip bgp *"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="/clear_bgp/clear-all=true">

                    <PARAM name="ipv4-ipv6-option"
                    help=""
                    ptype="SUBCOMMAND"
                    mode="switch"
                    optional="true">
                        <PARAM name="ipv4"
                        help="clear ip bgp ipv4"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                            <PARAM name="unicast"
                            help="clear ip bgp ipv4 unicast"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="/clear_bgp/address=0.0.0.0,/clear_bgp/address-family=ipv4,/clear_bgp/safi-index=unicast"/>
                        </PARAM>
                        <PARAM name="ipv6"
                        help="clear ip bgp ipv6"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
                            <PARAM name="unicast"
                            help="clear ip bgp ipv6 unicast"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="/clear_bgp/address=0::0,/clear_bgp/address-family=ipv6,/clear_bgp/safi-index=unicast"/>
                        </PARAM>
                        <PARAM name="soft"
                        help="Soft reconfiguration"
                        ptype="SUBCOMMAND"
                        mode="subcommand">
						    <PARAM name="soft-option"
						     help="Soft reconfig"
						     ptype="SUBCOMMAND"
						     mode="switch">
                                <PARAM name="in"
                                help="Soft reconfigure inbound update"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="/clear_bgp/address=0.0.0.0,/clear_bgp/soft-reconfig=in"/>
                                <PARAM name="out"
                                pfeature="NOT_SUPPORTED"
                                help="Soft reconfigure outbound update"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="/clear_bgp/address=0.0.0.0,/clear_bgp/soft-reconfig=out"/>
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM name="dampening"
                help="Clear route flap dampening information"
                ptype="SUBCOMMAND"
                mode="subcommand">
                    <PARAM name="ip"
                    help="Network to clear dampening statistics"
                    ptype="IPV4_IPV6_NETWORK"
                    yang_name="/clear_dampening_stats/vrf-name=default,/clear_dampening_stats/prefix=new_param1"
                    prompt_oper="${ip}"
                    prompt_fn="show_trim_ip"/>
                </PARAM>
                <PARAM name="flap-statistics"
                help="Clear route flap statistics"
                ptype="SUBCOMMAND"
                yang_name="/clear_flap_stats/vrf-name=default,/clear_flap_stats/clear-all=true"
                mode="subcommand">
                    <PARAM name="flap-commands"
                    help="bgp-sub commands"
                    optional="true"
                    ptype="SUBCOMMAND"
                    mode="switch">
                        <PARAM name="ip"
                        help="IPv4/IPv6  Network to clear flap statistics"
                        ptype="IPV4_IPV6_NETWORK"
                        refresh_yang_name="true"
                        yang_name="/clear_flap_stats/vrf-name=default,/clear_flap_stats/prefix=new_param1"
                        prompt_oper="${ip}"
                        prompt_fn="show_trim_ip"/>
                    </PARAM>
                </PARAM>
            </PARAM>
        <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
    </VIEW>
</CLISH_MODULE>
