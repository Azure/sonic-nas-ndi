<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
<!--=========================================================================-->

<VIEW name="enable-view">
    <!-- alias <alias-name> <alias-value> -->
    <COMMAND name="alias"
        help="Set alias for a command"
        >
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20">
            <PARAM name="alias-value"
                help="Value of the alias"
                ptype="STRING_WITH_PIPE"/>
        </PARAM>
        <ACTION builtin="clish_nop"></ACTION>
    </COMMAND>

    <!-- no alias <alias-name> -->
    <COMMAND name="no alias"
        help="Remove an alias definition">
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20">
        </PARAM>
        <ACTION builtin="clish_nop"></ACTION>
    </COMMAND>

    <!-- show alias -->
    <COMMAND name="show alias"
         help="Show list of aliases">

        <PARAM name="show-options"
            help="show options"
            ptype="SUBCOMMAND"
            mode="switch"
            optional="true"
            >
            <PARAM name="brief"
                help="Brief information of aliases"
                ptype="SUBCOMMAND"
                mode="subcommand"
            />
            <PARAM name="detail"
                help="Detail information of aliases"
                ptype="SUBCOMMAND"
                mode="subcommand"
            />
        </PARAM>

         <ACTION builtin="clish_nop"/>

    </COMMAND>
</VIEW>

<VIEW name="configure-view">

    <!-- feature <feature-name> -->
    <COMMAND name="feature"
        help="Enable feature"
        >

        <PARAM name="feature-name-options"
            help="feature names"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM name="config-os9-style"
                help="Enable OS9 style configuration commands"
                ptype="SUBCOMMAND"
                mode="subcommand"
            />
        </PARAM>

        <ACTION builtin="clish_nop"> </ACTION>

    </COMMAND>

    <!-- no feature <feature-name> -->
    <COMMAND name="no feature"
        help="Disable feature"
        >

        <PARAM name="feature-name-options"
            help="feature names"
            ptype="SUBCOMMAND"
            mode="switch"
            >
            <PARAM name="config-os9-style"
                help="Disable OS9 style configuration commands"
                ptype="SUBCOMMAND"
                mode="subcommand"
            />
        </PARAM>

        <ACTION builtin="clish_nop"> </ACTION>

    </COMMAND>

    <!-- alias <alias-name> <alias-value> -->
    <COMMAND name="alias"
        help="Set alias for a command"
        >
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20"
            >
            <PARAM name="alias-value"
                help="Value of the alias"
                ptype="STRING_WITH_PIPE"
                />
        </PARAM>
        <ACTION builtin="clish_nop"></ACTION>
    </COMMAND>
    <!-- no alias <alias-name> -->
    <COMMAND name="no alias"
        help="Remove an alias definition"
        >
        <PARAM name="alias-name"
            help="Name of the alias (Max size 20)"
            ptype="STRING_20"
            >
        </PARAM>
        <ACTION builtin="clish_nop"></ACTION>
    </COMMAND>

</VIEW>

</CLISH_MODULE>
