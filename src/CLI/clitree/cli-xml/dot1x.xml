                            help="Delete the host mode"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            />
                        <PARAM
                            name="multi-auth"
                            help="Delete the host mode"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            >
                        </PARAM>
                    </PARAM>
                </PARAM>
                <!--dot1x max-req -->
                <PARAM
                    name="max-req"
