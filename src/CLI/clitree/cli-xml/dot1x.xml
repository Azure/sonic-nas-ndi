                        <!--dot1x port-control auto -->
                        <PARAM
                            name="auto"
                            help="optional mode value "
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            />
                        <!--dot1x port-control force-authorized -->
                        <PARAM
                            name="force-authorized"
                            help="optional mode value "
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            />
                        <!--dot1x port-control force-unauthorized -->
                        <PARAM
                            name="force-unauthorized"
                            help="optional mode value "
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            />
                    </PARAM>
                </PARAM>
                <!--dot1x re-authentication -->
                <PARAM
                    name="re-authentication"
                    help="disable reauthentication in the interface"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="&DOT1X_CONFIG_IF_NAME;,&DOT1X_CONFIG_IF_PARAMS;/dell-dot1x:reauth-enabled"
                    sub_oper_yang = "remove:reauth-enabled"
                    />
                <PARAM
                    name="timeout"
                    help="Remove timeout value"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    >
                    <PARAM
                        name="timeout-options"
                        help="Remove timeout value options"
                        ptype="SUBCOMMAND"
                        mode="switch"
                        >
                        <!--dot1x timeout quiet-period -->
                        <PARAM
                            name="quiet-period"
                            help="select quiet-period option to remove"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&DOT1X_CONFIG_IF_NAME;,&DOT1X_CONFIG_IF_PARAMS;/dell-dot1x:quiet-period"
                            sub_oper_yang = "remove:quiet-period"
                            >
                            <PARAM
                                name="quiet-period-value"
                                optional="true"
                                help="Optional value in seconds"
                                ptype="RANGE_1_65535"
                                />
                        </PARAM>
                        <!--dot1x timeout re-authperiod -->
                        <PARAM
                            name="re-authperiod"
                            help="select re-authperiod as property to remove"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&DOT1X_CONFIG_IF_NAME;,&DOT1X_CONFIG_IF_PARAMS;/dell-dot1x:reauth-period"
                            sub_oper_yang = "remove:reauth-period"
                            >
                            <PARAM
                                name="re-authperiod-value"
                                help="Optional value in seconds"
                                optional="true"
                                ptype="RANGE_1_65535"
                                />
                        </PARAM>
                        <!--dot1x timeout server-timeout -->
                        <PARAM
                            name="server-timeout"
                            help="select re-authperiod as property to remove"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&DOT1X_CONFIG_IF_NAME;,&DOT1X_CONFIG_IF_PARAMS;/dell-dot1x:server-timeout"
                            sub_oper_yang = "remove:server-timeout"
                            >
                            <PARAM
                                name="server-timeout-value"
                                optional="true"
                                help="Optional value in seconds"
                                ptype="RANGE_1_65535"
                                />
                        </PARAM>
                        <!--dot1x timeout supp-timeout -->
                        <PARAM
                            name="supp-timeout"
                            help="select supp-timeout as property to remove"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&DOT1X_CONFIG_IF_NAME;,&DOT1X_CONFIG_IF_PARAMS;/dell-dot1x:supp-timeout"
                            sub_oper_yang = "remove:supp-timeout"
                            >
                            <PARAM
                                name="supp-timeout-value"
                                optional="true"
                                help="Optional value in seconds"
                                ptype="RANGE_1_65535"
                                />
                        </PARAM>
                        <!--dot1x timeout tx-period -->
                        <PARAM
                            name="tx-period"
                            help="select tx-period as property to remove"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            yang_name="&DOT1X_CONFIG_IF_NAME;,&DOT1X_CONFIG_IF_PARAMS;/dell-dot1x:tx-period"
                            sub_oper_yang = "remove:tx-period"
                            >
                            <PARAM
                                name="tx-period-value"
                                help="Optional value in seconds"
                                optional="true"
                                ptype="RANGE_1_65535"
                                />
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION
                builtin="clish_extn"
                > </ACTION>
        </COMMAND>
    </VIEW>
