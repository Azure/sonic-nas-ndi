<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY MODULE-NS-SA "http://www.dellemc.com/networking/os10/dell-support-assist">
<!ENTITY FILTER_SA "Filter_Type:SUBTREE;Filter_Namespace:&MODULE-NS-SA;;Filter_Conditions:">
<!ENTITY SCHEDULE_LIST "/sa-cfg/activity-schedule/activity-schedule-list">
<!ENTITY SCHEDULE_ACTIVITY "/activity-launch">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                                                http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!-- ================================================== -->
    <VIEW
        name="enable-view"
        >
         <!--show running-configuration support-assist -->
        <COMMAND
            name="show running-configuration support-assist"
            help="Current operating support-assist configuration"
            yang_filter="&FILTER_SA;/sa-cfg"
            db_mode="configure-supportassist"
            >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <!--show candidate-configuration support-assist -->
        <COMMAND
            name="show candidate-configuration support-assist"
            help="Current candidate support-assist configuration"
            yang_filter="&FILTER_SA;/sa-cfg"
            db_mode="configure-supportassist"
            >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="show support-assist"
            help="Shows information about the support assist module"
            operation="get">
            <PARAM
                name="status"
                help="Shows status information for support-assist module"
                namespace="&MODULE-NS-SA;"
                ptype="SUBCOMMAND"
                mode="subcommand"
                template="show_support-assist_status.xsl"
                operation="get"
                yang_filter="&FILTER_SA;/sa-cfg|&FILTER_SA;/sa-status-info"
                >
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="show support-assist eula"
            help="Shows eula for support-assist module"
            operation="get"
            >
            <ACTION>less /opt/dell/os10/eula/eula-support-assist.txt </ACTION>
        </COMMAND>
        <!-- Launch an activity -->
        <COMMAND
            name="no support-assist-activity"
            help="Support Assist related activity"
            ptype="SUBCOMMAND"
            operation="generic-rpc"
            namespace="&MODULE-NS-SA;"
            prompt_fn="sa_xfer_check"
            prompt_oper="&FILTER_SA;/sa-cfg"
            mode="subcommand"
            default_oper="merge"
            >
            <PARAM
                name="activity_name"
                help="activity name"
                ptype="SA_SCHED_ACTIVITY_TYPE"
                yang_name="/activity-launch/type=${activity_name}"
                >
                <PARAM
                    name="schedule"
                    help="remove a schedule"
                    ptype="SUBCOMMAND"
                    mode="subcommand"
                    yang_name="/activity-launch/schedule=false"
                />
            </PARAM>
            <ACTION builtin="clish_extn"/>
         </COMMAND>
         <COMMAND
            name="support-assist-activity"
            help="Support Assist related activity"
            ptype="SUBCOMMAND"
            operation="generic-rpc"
            namespace="&MODULE-NS-SA;"
            prompt_fn="sa_xfer_check"
            prompt_oper="&FILTER_SA;/sa-cfg"
            mode="subcommand"
            default_oper="merge"
            >
            <PARAM
                name="activity_name"
                help="activity name"
                ptype="SA_SCHED_ACTIVITY_TYPE"
                yang_name="&SCHEDULE_ACTIVITY;/type=${activity_name}"
                >
                <PARAM
                    name="when-option"
                    help="switch for scheduling option"
                    ptype="SUBCOMMAND"
                    mode="switch"
                    >
                    <PARAM
                        name="start-now"
                        help="launch an activity now"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        template="show_support-assist_activity_launch.xsl"
                        yang_name="&SCHEDULE_ACTIVITY;/start-now=true"
                    />
                    <PARAM
                        name="schedule"
                        help="schedule an activity"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        template="show_support-assist_activity_launch.xsl"
                        yang_name="&SCHEDULE_ACTIVITY;/schedule=true"
                        >
                        <PARAM
                            name="schedule-option"
                            help="switch for scheduling option"
                            ptype="SUBCOMMAND"
                            mode="switch"
                            >
                            <PARAM
                                name="hourly"
                                help="schedule an hourly task"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="&SCHEDULE_ACTIVITY;/frequency=hourly"
                                >
                                <PARAM
                                    name="min"
                                    help="schedule at this minute"
                                    ptype="SUBCOMMAND"
                                    mode="subcommand">
                                    <PARAM
                                        name="min_val"
                                        help=""
                                        ptype="MINUTES_TYPE"
                                        yang_name="&SCHEDULE_ACTIVITY;/minute=${min_val}"
                                    />
                                </PARAM>
                            </PARAM>
                            <PARAM
                                name="daily"
                                help="schedule a daily task"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="&SCHEDULE_ACTIVITY;/frequency=daily"
                                >
                                <PARAM
                                   name="hour"
                                   help="schedule at this hour"
                                   ptype="SUBCOMMAND"
                                   mode="subcommand">
                                   <PARAM
                                       name="hour_val"
                                       help=""
                                       ptype="HOURS_TYPE"
                                       yang_name="&SCHEDULE_ACTIVITY;/hour=${hour_val}"
                                       />
                                   <PARAM
                                      name="min"
                                      help="schedule at this minute"
                                      ptype="SUBCOMMAND"
                                      mode="subcommand">
                                      <PARAM
                                          name="min_val"
                                          help=""
                                          ptype="MINUTES_TYPE"
                                          yang_name="&SCHEDULE_ACTIVITY;/minute=${min_val}"
                                          />
                                   </PARAM>
                                </PARAM>
                            </PARAM>
                            <PARAM
                                name="weekly"
                                help="schedule a weekly task"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="&SCHEDULE_ACTIVITY;/frequency=weekly"
                                >
                                <PARAM
                                   name="day-of-week"
                                   help="schedule at this day of week"
                                   ptype="SUBCOMMAND"
                                   mode="subcommand">
                                   <PARAM
                                       name="day_of_week_val"
                                       help=""
                                       ptype="DAYS_OF_WEEK_TYPE"
                                       yang_name="&SCHEDULE_ACTIVITY;/day-of-week=${day_of_week_val}"
                                       />
                                   <PARAM
                                       name="hour"
                                       help="schedule at this hour"
                                       ptype="SUBCOMMAND"
                                       mode="subcommand">
                                       <PARAM
                                           name="hour_val"
                                           help=""
                                           ptype="HOURS_TYPE"
                                           yang_name="&SCHEDULE_ACTIVITY;/hour=${hour_val}"
                                           />
                                       <PARAM
                                           name="min"
                                           help="schedule at this minute"
                                           ptype="SUBCOMMAND"
                                           mode="subcommand">
                                           <PARAM
                                               name="min_val"
                                               help=""
                                               ptype="MINUTES_TYPE"
                                               yang_name="&SCHEDULE_ACTIVITY;/minute=${min_val}"
                                               />
                                        </PARAM>
                                    </PARAM>
                                </PARAM>
                            </PARAM>
                            <PARAM
                                name="monthly"
                                help="schedule a monthly task"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="&SCHEDULE_ACTIVITY;/frequency=monthly"
                                >
                                <PARAM
                                   name="day"
                                   help="schedule at this day"
                                   ptype="SUBCOMMAND"
                                   mode="subcommand">
                                   <PARAM
                                       name="day_val"
                                       help=""
                                       ptype="DAYS_TYPE"
                                       yang_name="&SCHEDULE_ACTIVITY;/day=${day_val}"
                                       />
                                   <PARAM
                                       name="hour"
                                       help="schedule at this hour"
                                       ptype="SUBCOMMAND"
                                       mode="subcommand">
                                       <PARAM
                                           name="hour_val"
                                           help=""
                                           ptype="HOURS_TYPE"
                                           yang_name="&SCHEDULE_ACTIVITY;/hour=${hour_val}"
                                           />
                                       <PARAM
                                           name="min"
                                           help="schedule at this minute"
                                           ptype="SUBCOMMAND"
                                           mode="subcommand">
                                           <PARAM
                                               name="min_val"
                                               help=""
                                               ptype="MINUTES_TYPE"
                                               yang_name="&SCHEDULE_ACTIVITY;/minute=${min_val}"
                                               />
                                        </PARAM>
                                    </PARAM>
                                </PARAM>
                            </PARAM>
                            <PARAM
                                name="yearly"
                                help="schedule a yearly task"
                                ptype="SUBCOMMAND"
                                mode="subcommand"
                                yang_name="&SCHEDULE_ACTIVITY;/frequency=yearly"
                                >
                                <PARAM
                                    name="month"
                                    help="schedule in this month"
                                    ptype="SUBCOMMAND"
                                    mode="subcommand">
                                    <PARAM
                                        name="month_val"
                                        help=""
                                        ptype="MONTHS_TYPE"
                                        yang_name="&SCHEDULE_ACTIVITY;/month=${month_val}"
                                        />
                                    <PARAM
                                        name="day"
                                        help="schedule at this day"
                                        ptype="SUBCOMMAND"
                                        mode="subcommand">
                                        <PARAM
                                            name="day_val"
                                            help=""
                                            ptype="DAYS_TYPE"
                                            yang_name="&SCHEDULE_ACTIVITY;/day=${day_val}"
                                            />
                                        <PARAM
                                            name="hour"
                                            help="schedule at this hour"
                                            ptype="SUBCOMMAND"
                                            mode="subcommand">
                                            <PARAM
                                                name="hour_val"
                                                help=""
                                                ptype="HOURS_TYPE"
                                                yang_name="&SCHEDULE_ACTIVITY;/hour=${hour_val}"
                                                />
                                            <PARAM
                                                name="min"
                                                help="schedule at this minute"
                                                ptype="SUBCOMMAND"
                                                mode="subcommand">
                                                <PARAM
                                                    name="min_val"
                                                    help=""
                                                    ptype="MINUTES_TYPE"
                                                    yang_name="&SCHEDULE_ACTIVITY;/minute=${min_val}"
                                                    />
                                            </PARAM>
                                        </PARAM>
                                    </PARAM>
                                </PARAM>
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
         </COMMAND>
     </VIEW>
    <!-- =================================================== -->
    <VIEW
        name="configure-view"
        >
        <COMMAND
            name="support-assist"
            help="Support Assist feature configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            prompt_fn="eula_support"
            prompt_oper="&FILTER_SA;/sa-cfg/eula-accept"
            view="configure-supportassist-view"
            yang_name="/sa-cfg/service-enable=true"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>
    <!-- support-assist view -->
    <VIEW
        name="configure-supportassist-view"
        prompt="${SYSTEM_NAME}(conf-support-assist)# "
        depth="2"
        >
        <NAMESPACE
            ref="configure-view"
            help="false"
            completion="false"
            />
        <!-- show configuration candidate -->
        <COMMAND
             name="show"
             help="show configuration"
             />
        <COMMAND
             name="show configuration candidate"
             help="Current candidate configuration"
             yang_filter="&FILTER_SA;/sa-cfg"
             >
             <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <!-- show configuration -->
        <COMMAND
             name="show configuration"
             help="Current configuration (default = running)"
             yang_filter="&FILTER_SA;/sa-cfg"
             >
             <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="no"
            help="Negate a command or set its defaults"
            />
        <!-- Server Info -->
        <COMMAND
            name="server"
            help="Support Assist remote server configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            >
            <PARAM
                name="url"
                help="url configuration"
                mode="subcommand"
                ptype="SUBCOMMAND"
                >
                <PARAM
                    name="server_url"
                    help=" url to reach support assist server"
                    ptype="SA_URL"
                    yang_name="/sa-cfg/sa-server/server-url=${server_url}"
                    >
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no server"
            help="Remove server"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:sa-server"
            yang_name="/sa-cfg/sa-server"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <!-- Proxy server settings -->
        <COMMAND
            name="proxy-server"
            help="proxy-server configuration to reach Support Assist remote server"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            >
            <PARAM
                name="ip"
                help="proxy ip address"
                mode="subcommand"
                ptype="SUBCOMMAND"
                >
                <PARAM
                    name="ip_addr"
                    help="proxy-ip address"
                    ptype="IPV4_OR_IPV6_ADDR"
                    yang_name="/sa-cfg/proxy-server/ip-address=${ip_addr}"
                    >
                    <PARAM
                        name="port"
                        help="proxy port"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        >
                        <PARAM
                            name="port_num"
                            help="port-number for the proxy"
                            ptype="UINT16"
                            yang_name="/sa-cfg/proxy-server/port-number=${port_num}"
                            >
                            <PARAM
                                name="username"
                                help="proxy username"
                                mode="subcommand"
                                ptype="SUBCOMMAND"
                                optional="true"
                                >
                                <PARAM
                                    name="user_name"
                                    help="username for the proxy"
                                    ptype="STRING_1_140"
                                    yang_name="/sa-cfg/proxy-server/username=${user_name}"
                                    >
                                    <PARAM
                                        name="password"
                                        help="proxy password"
                                        mode="subcommand"
                                        ptype="SUBCOMMAND"
                                        >
                                        <PARAM
                                            name="pwd"
                                            help="password for the proxy"
                                            ptype="STRING_1_140"
                                            yang_name="/sa-cfg/proxy-server/password=${pwd}"
                                            >
                                        </PARAM>
                                    </PARAM>
                                </PARAM>
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no proxy-server"
            help="Remove proxy server configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:proxy-server"
            yang_name="/sa-cfg/proxy-server"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <!-- Activity settings -->
        <COMMAND
            name="activity"
            help="activity configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            add_config_entry="false"
            yang_key="/sa-cfg/activity-enable/activity-enable-list/type"
            >
            <PARAM
                name="act_nm"
                help="activity name"
                ptype="SA_ACTIVITY_TYPE"
                yang_name="/sa-cfg/activity-enable/activity-enable-list/type=${act_nm}"
                >
                <PARAM
                    name="act_enable"
                    help="activity enable subcommand"
                    mode="switch"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="enable"
                        help="enable activity(trigger based or on scheduled time)"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        yang_name="/sa-cfg/activity-enable/activity-enable-list/enabled=true"
                        />
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no activity"
            help="activity configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            default_oper="merge"
            add_config_entry="true"
            yang_key="/sa-cfg/activity-enable/activity-enable-list/type"
            >
            <PARAM
                name="act_nm"
                help="support assist activity name"
                ptype="SA_ACTIVITY_TYPE"
                yang_name="/sa-cfg/activity-enable/activity-enable-list/type=${act_nm}"
                >
                <PARAM
                     name="act_enable"
                     help="activity enable subcommand"
                     mode="switch"
                     ptype="SUBCOMMAND"
                     >
                     <PARAM
                          name="enable"
                          help="enable activity(trigger based or on scheduled time)"
                          mode="subcommand"
                          ptype="SUBCOMMAND"
                          yang_name="/sa-cfg/activity-enable/activity-enable-list/enabled=false"
                          />
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <!-- contact Company Info -->
        <COMMAND
            name="contact-company"
            help="contact-company configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            view="configure-supportassist-contactcompany-view"
            viewid="ct_comp=${c_name}"
            >
            <PARAM
                name="name"
                help="contact-company name details"
                mode="subcommand"
                ptype="SUBCOMMAND"
                >
                <PARAM
                    name="c_name"
                    help="contact company name"
                    ptype="STRING_1_140"
                    yang_name="/sa-cfg/company-info/name=${c_name}"
                    />
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no contact-company"
            help="Remove contact information for the company"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:company-info"
            yang_name="/sa-cfg/company-info"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>

    <!-- contact company view -->
    <VIEW
        name="configure-supportassist-contactcompany-view"
        prompt="${SYSTEM_NAME}(conf-support-assist-${ct_comp})# "
        depth="3"
        >
        <NAMESPACE
            ref="configure-view"
            completion="false"
            help="false"
            />
        <!-- show configuration candidate -->
        <COMMAND
             name="show"
             help="show configuration"
             />
        <COMMAND
             name="show configuration candidate"
             help="Current candidate configuration"
             yang_filter="&FILTER_SA;/sa-cfg/company-info"
             >
             <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <!-- show configuration -->
        <COMMAND
             name="show configuration"
             help="Current configuration (default = running)"
             yang_filter="&FILTER_SA;/sa-cfg/company-info"
             >
             <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="no"
            help="Negate a command or set its defaults"
            />
        <COMMAND
            name="street-address"
            help="street-address configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config">
            <PARAM
                name="st_addr1"
                help="street-address line 1 for company"
                ptype="STRING_1_140"
                yang_name="/sa-cfg/company-info/street-address=${st_addr1}"
                >
                <PARAM
                    name="st_addr2"
                    help="street-address line 2 for company"
                    ptype="STRING_1_140"
                    yang_name="/sa-cfg/company-info/street-address-line2=${st_addr2}"
                    optional="true"
                    >
                    <PARAM
                        name="st_addr3"
                        help="street-address line 3 for company"
                        ptype="STRING_1_140"
                        yang_name="/sa-cfg/company-info/street-address-line3=${st_addr3}"
                        optional="true"
                        >
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no street-address"
            help="remove street-address configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:street-address,remove:street-address-line2,remove:street-address-line3"
            yang_name="/sa-cfg/company-info/street-address, /sa-cfg/company-info/street-address-line2,/sa-cfg/company-info/street-address-line3"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>

        <COMMAND
            name="address"
            help="address configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            >
            <PARAM
                name="city"
                help="city details"
                mode="subcommand"
                ptype="SUBCOMMAND"
                >
                <PARAM
                    name="city_name"
                    help="city name for company"
                    ptype="STRING_1_140"
                    yang_name="/sa-cfg/company-info/city=${city_name}"
                    >
                    <PARAM
                        name="state"
                        help="state details"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        >
                        <PARAM
                            name="state_name"
                            help="state name for company"
                            ptype="STRING_1_140"
                            yang_name="/sa-cfg/company-info/state=${state_name}"
                            >
                            <PARAM
                                name="country"
                                help="country details"
                                mode="subcommand"
                                ptype="SUBCOMMAND"
                                >
                                <PARAM
                                    name="country_name"
                                    help="country name for company"
                                    ptype="STRING_1_140"
                                    yang_name="/sa-cfg/company-info/country=${country_name}"
                                    >
                                    <PARAM
                                        name="zipcode"
                                        help="zipcode details"
                                        mode="subcommand"
                                        ptype="SUBCOMMAND"
                                        >
                                        <PARAM
                                            name="zip"
                                            help="zipcode for company"
                                            ptype="STRING_1_140"
                                            yang_name="/sa-cfg/company-info/zipcode=${zip}"
                                            >
                                        </PARAM>
                                    </PARAM>
                                </PARAM>
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no address"
            help="remove address configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:city,remove:state,remove:country,remove:zipcode"
            yang_name="/sa-cfg/company-info/city,/sa-cfg/company-info/state,/sa-cfg/company-info/country,/sa-cfg/company-info/zipcode"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="territory"
            help="territory configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            >
            <PARAM
                name="trty"
                help="territory for company"
                ptype="STRING_1_140"
                yang_name="/sa-cfg/company-info/territory=${trty}"
                />
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no territory"
            help="remove territory configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:territory"
            yang_name="/sa-cfg/company-info/territory"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <!-- contact person info -->
        <COMMAND
            name="contact-person"
            help="contact-person configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            view="configure-supportassist-contactcompany-contactperson-view"
            viewid="p_name=${ct_comp}-${first_name}${last_name}"
            >
            <PARAM
                name="first"
                help="contact-person first name"
                mode="subcommand"
                ptype="SUBCOMMAND"
                >
                <PARAM
                    name="first_name"
                    help="first name for contact person"
                    ptype="STRING_1_140"
                    yang_name="/sa-cfg/company-info/contact-person/first-name=${first_name}"
                    >
                    <PARAM
                        name="last"
                        help="contact-person last name"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        >
                        <PARAM
                            name="last_name"
                            help="last name for contact person"
                            ptype="STRING_1_140"
                            yang_name="/sa-cfg/company-info/contact-person/last-name=${last_name}"
                            >
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no contact-person"
            help="Remove contact-person configuartion"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:contact-person"
            yang_name="/sa-cfg/company-info/contact-person"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>

    <!-- server view-->
    <!-- contact person view-->
    <VIEW
        name="configure-supportassist-contactcompany-contactperson-view"
        prompt="${SYSTEM_NAME}(conf-support-assist-${p_name})# "
        depth="4"
        >
        <NAMESPACE
            ref="configure-view"
            completion="false"
            help="false"
            />
        <!-- show configuration candidate -->
        <COMMAND
             name="show"
             help="show configuration"
             />
        <COMMAND
             name="show configuration candidate"
             help="Current candidate configuration"
             yang_filter="&FILTER_SA;/sa-cfg/company-info/contact-person"
             >
             <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <!-- show configuration -->
        <COMMAND
             name="show configuration"
             help="Current configuration (default = running)"
             yang_filter="&FILTER_SA;/sa-cfg/company-info/contact-person"
             >
             <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>
        <COMMAND
            name="no"
            help="Negate a command or set its defaults"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="email-address"
            help="email address configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            >
            <PARAM
                name="email"
                help="email for contact person"
                ptype="EMAILID"
                yang_name="/sa-cfg/company-info/contact-person/email-address=${email}"
                >
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no email-address"
            help="Remove email address configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:email-address"
            yang_name="/sa-cfg/company-info/contact-person/email-address"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="phone"
            help="phone configuration"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            >
            <PARAM
                name="primary"
                help="contact-person primary phone"
                mode="subcommand"
                ptype="SUBCOMMAND"
                >
                <PARAM
                    name="p_phone"
                    help="primary phone for contact person"
                    ptype="STRING_1_140"
                    yang_name="/sa-cfg/company-info/contact-person/phone=${p_phone}"
                    >
                    <PARAM
                        name="alternate"
                        help="contact-person alternate phone"
                        mode="subcommand"
                        ptype="SUBCOMMAND"
                        optional="true"
                        >
                        <PARAM
                            name="a_phone"
                            help="alternate phone for contact person"
                            ptype="STRING_1_140"
                            yang_name="/sa-cfg/company-info/contact-person/alternate-phone=${a_phone}"
                            >
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no phone"
            help="remove phone configuartion"
            namespace="&MODULE-NS-SA;"
            operation="edit-config" sub_oper_yang="remove:phone,remove:alternate-phone"
            yang_name="/sa-cfg/company-info/contact-person/phone,/sa-cfg/company-info/contact-person/alternate-phone"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="preferred-method"
            help="configure preferred-method of contact"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            >
            <PARAM
                name="preferred-method-sub-commands"
                help="preferred-method sub-commands"
                mode="switch"
                ptype="SUBCOMMAND"
                >
                <PARAM
                    name="email"
                    help="configure email as preferred-method of contact"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name="/sa-cfg/company-info/contact-person/preferred-method=email"
                    />
                <PARAM
                    name="phone"
                    help="configure phone as preferred-method of contact"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name="/sa-cfg/company-info/contact-person/preferred-method=phone"
                    />
                <PARAM
                    name="no-contact"
                    help="configure no-contact as preferred-method of contact"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    yang_name="/sa-cfg/company-info/contact-person/preferred-method=no-contact"
                    />
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        <COMMAND
            name="no preferred-method"
            help="remove preferred-method of contact"
            namespace="&MODULE-NS-SA;"
            operation="edit-config"
            sub_oper_yang="remove:preferred-method"
            yang_name="/sa-cfg/company-info/contact-person/preferred-method"
            >
            <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>
</CLISH_MODULE>
