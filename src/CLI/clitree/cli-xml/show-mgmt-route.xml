<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY SUBTREE_FILTER                         "Filter_Type:SUBTREE">
<!ENTITY FILTER_NS                              "Filter_Namespace:http://www.dellemc.com/networking/os10/dell-management-routing">
<!ENTITY MGMT_RIB                               "/mgmt-if-route-oper/mgmt-rib">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd">
<!-- ********************************************************************** -->
<!-- *  SHOW IP MANAGEMENT ROUTE COMMANDS                                 * -->
<!--
show ip management route all
show ipv6 management route all

show ip management route connected
show ipv6 management route connected

show ip management route summary
show ipv6 management route summary

show ip management route static
show ipv6  management route static
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->
    <VIEW name="enable-view">
        <COMMAND name="show ip management-route"
                 help="Management IP route information"
                 operation="get"
                 yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV4,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route"
                 template="show_mgmt_route.xsl"
                 >
                 <PARAM name="type"
                        help="switch subcommand"
                        ptype="SUBCOMMAND"
                        optional="true"
                        mode="switch"
                        >
                        <PARAM name="all"
                               help="All management routes including non-active routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV4,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route"
                               template="show_mgmt_route_all.xsl"
                               />
                        <PARAM name="connected"
                               help="All Management interface connected routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV4,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route/source=source-connected"
                               template="show_mgmt_route.xsl"
                               />
                        <PARAM name="summary"
                               help="Summary of Management interface  routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV4,&MGMT_RIB;/safi=nlriUnicast"
                               template="show_mgmt_route_summary.xsl"
                               />
                        <PARAM name="static"
                               help="All Management interface static active routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV4,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route/source=source-static"
                               template="show_mgmt_route.xsl"
                               />
                 </PARAM>
             <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
         <COMMAND name="show ipv6 management-route"
                 help="Management IP route information"
                 operation="get"
                 yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV6,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route"
                 template="show_mgmt_v6_route.xsl"
                 >
                 <PARAM name="type"
                        help="switch subcommand"
                        ptype="SUBCOMMAND"
                        optional="true"
                        mode="switch"
                        >
                        <PARAM name="all"
                               help="All Management interface routes including non-active routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV6,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route"
                               template="show_mgmt_v6_route_all.xsl"
                               />
                        <PARAM name="connected"
                               help="All Management interface connected routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV6,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route/source=source-connected"
                               template="show_mgmt_v6_route.xsl"
                               />
                        <PARAM name="summary"
                               help="Summary of Management interface  routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV6,&MGMT_RIB;/safi=nlriUnicast"
                               template="show_mgmt_route_summary.xsl"
                               />
                        <PARAM name="static"
                               help="All Management interface static active routes"
                               ptype="SUBCOMMAND"
                               mode="subcommand"
                               yang_filter="&SUBTREE_FILTER;;&FILTER_NS;;Filter_Conditions:&MGMT_RIB;/afn=ipV6,&MGMT_RIB;/safi=nlriUnicast,&MGMT_RIB;/route/source=source-static"
                               template="show_mgmt_v6_route.xsl"
                               />
                 </PARAM>
             <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
    </VIEW>
</CLISH_MODULE>


