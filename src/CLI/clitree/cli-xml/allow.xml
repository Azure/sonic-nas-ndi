<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY NAME_SPACE     "http://www.dellemc.com/networking/os10/dell-equipment">
<!ENTITY SUBTREE_FILTER "Filter_Type:SUBTREE">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->
    <VIEW
        name="hidden-view"
        >
        <!-- allow -->
        <COMMAND
            name="allow"
            help="Allow commands"
            />
        <!-- allow unsupported-transceiver -->
        <COMMAND
            name="allow unsupported-transceiver"
            operation="generic-rpc"
            namespace="&NAME_SPACE;"
            help="Disable lockout of unqualified optics"
            yang_name="/set-media-optic/disable-lockout=true">
            <ACTION builtin="clish_extn"/>
        </COMMAND>

        <!-- no allow -->
        <COMMAND
            name="no allow"
            help="Disallow commands"
            />
        <!-- no allow unsupported-transceiver -->
        <COMMAND
            name="no allow unsupported-transceiver"
            operation="generic-rpc"
            namespace="&NAME_SPACE;"
            help="Enable lockout of unqualified optics"
            yang_name="/set-media-optic/disable-lockout=false">
            <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>
    <!--=======================================================-->
    <!--                Config MODE                            -->
    <!--=======================================================-->
</CLISH_MODULE>
