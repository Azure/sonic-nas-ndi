    <!--=======================================================-->
    <!--                Config logging server MODE             -->
    <!--=======================================================-->

    <!-- commands for this level are not required yet          -->
    <VIEW
        name="configure-logging-server-view"
        prompt="${SYSTEM_NAME}(config-logging-server-${v_id})# "
        depth="2"
        >
        <!-- Inheritance -->
        <NAMESPACE
            ref="configure-view"
            help="false"
