<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY NAME_SPACE                     "http://www.dellemc.com/networking/os10/dell-logging">
<!ENTITY SUBTREE_FILTER                 "Filter_Type:SUBTREE">
<!ENTITY FILTER_LOGGING                 "&SUBTREE_FILTER;;Filter_Namespace:&NAME_SPACE;;Filter_Conditions:">
<!ENTITY LOGGING_REMOTE_HOST            "/logging-trace/logging/logging-remote/remote-host-ip-address">
<!ENTITY LOGGING_REMOTE_HOST_LEVEL      "/logging-trace/logging/logging-remote/remote-host-level">
<!ENTITY LOGGING_REMOTE                 "/logging-trace/logging/logging-remote">
<!ENTITY LOGGING_LOGGING                "/logging-trace/logging/logging">
<!ENTITY LOGGING_LOGGING_MODULE         "/logging-trace/logging/logging/log-output-module">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->
    <VIEW name="enable-view">
    <!--show candidate-configuration logging -->
        <COMMAND
            name="show candidate-configuration logging"
            help="Current candidate logging configuration"
            yang_filter="&FILTER_LOGGING;/logging-trace"
            db_mode="configure"
            >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

        <!--show running-configuration logging -->
        <COMMAND
            name="show running-configuration logging"
            help="Current operating logging configuration"
            yang_filter="&FILTER_LOGGING;/logging-trace"
            db_mode="configure"
            >
            <ACTION builtin="clish_extn"> </ACTION>
        </COMMAND>

    <!--MODE : exec -->
    <!-- clear commands -->
      <COMMAND name="clear logging"
        help="Clear logging command"
        >
      </COMMAND>
      <COMMAND name="clear logging log-file"
         help="Clear syslog messages"
         ptype="SUBCOMMAND"
         mode="subcommand"
         operation="generic-rpc"
         namespace="&NAME_SPACE;"
         prompt_fn="prompt_yes_no_cb"
         prompt_str="Proceed to clear the log file [confirm yes/no(default)]:"
         yang_name="/log-buffer-clear"
         >
         <ACTION builtin="clish_extn"/>
      </COMMAND>

    <!--
      <COMMAND name="clear logging summary"
         help="Clear logging summary"
         ptype="SUBCOMMAND"
         mode="subcommand"
         operation="generic-rpc"
         namespace="&NAME_SPACE;"
         prompt_fn="prompt_yes_no_cb"
         prompt_str="Proceed to clear the log summary [confirm yes/no(default)]:"
         yang_name="/logging-severity-statistics-clear"
         >
         <ACTION builtin="clish_extn"/>
      </COMMAND>
    -->

    <!-- show commands -->
       <COMMAND
        name="show logging"
        help="Show logging messages"
        >
       </COMMAND>
       <COMMAND name="show logging log-file"
         help="Show syslog messages"
         >
         <PARAM
             name="lines"
             help="Number of messages to be displayed"
             ptype="RANGE_1_65535"
             optional="true"
             default="65535"
             >
         </PARAM>
	  <ACTION builtin="clish_file_print">tail -n ${lines} /var/log/eventlog 2>/dev/null</ACTION>
      </COMMAND>

       <COMMAND name="show logging log-file process-name"
         help="Show syslog messages filtered by process name"
         >
         <PARAM
             name="pname"
             help="Process name"
             ptype="STRING"
             >
         </PARAM>
	  <ACTION builtin="clish_file_print">bash /opt/dell/os10/bin/etl_svc_pname_filter.sh ${pname} 2>/dev/null</ACTION>
      </COMMAND>

       <COMMAND
         name="show logging process-names"
         help="Show logging process names"
         ptype="SUBCOMMAND"
         mode="subcommand"
         >
	 <ACTION builtin="clish_file_print">/opt/dell/os10/bin/etl_svc_show_pname 2>/dev/null</ACTION>
      </COMMAND>

      <COMMAND name="show trace"
         help="Show trace messages"
         >
         <PARAM
             name="lines"
             help="Number of messages to be displayed"
             ptype="RANGE_1_65535"
             optional="true"
             default="65535"
             >
         </PARAM>
         <ACTION builtin="clish_file_print">sudo journalctl -n ${lines} -q 2>/dev/null</ACTION>
      </COMMAND>

    <!--
      <COMMAND name="show logging summary"
         help="Show the counts of messages by severity level"
         operation="get"
         yang_filter="&FILTER_LOGGING;/logging-severity-statistics|&FILTER_LOGGING;/logging-severity-stats"
         template="show_logging_summary.xsl"
         >
         <ACTION builtin="clish_extn"> </ACTION>
      </COMMAND>
    -->

    </VIEW>
    <!--=======================================================-->
    <!--                Config MODE                            -->
    <!--=======================================================-->
    <VIEW
        name="configure-view"
        >
        <!-- logging -->
        <COMMAND
            name="logging"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            help="Logging commands"
            />
        <!-- logging enable -->
        <COMMAND
            name="logging enable"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            help="Enable logging to all supported destinations"
            add_config_entry = "false"
            yang_name="/logging-trace/global-log-enable=true"
            >
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
        <!-- no logging -->
        <COMMAND
            name="no logging"
            help="Disable all logging"
            />
        <!-- no logging enable-->
        <COMMAND
            name="no logging enable"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            default_oper="merge"
            add_config_entry = "true"
            help="Disable all logging"
            yang_name="/logging-trace/global-log-enable=false"
            >
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>

        <!-- trace -->
        <COMMAND
            name="trace"
            pfeature="NOT_SUPPORTED"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            help="Trace commands"
            />
        <!-- trace enable -->
        <COMMAND
            name="trace enable"
            pfeature="NOT_SUPPORTED"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            help="Enable trace all messages"
            add_config_entry = "false"
            yang_name="/logging-trace/global-trace-enable=true"
            >
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>
        <!-- no trace -->
        <COMMAND
            name="no trace"
            pfeature="NOT_SUPPORTED"
            help="Disable all traces"
            />
        <!-- no trace enable-->
        <COMMAND
            name="no trace enable"
            pfeature="NOT_SUPPORTED"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            default_oper="merge"
            add_config_entry = "true"
            help="Enables all traces"
            yang_name="/logging-trace/global-trace-enable=false"
            >
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>

        <!-- logging server <string> -->
        <COMMAND
            name="logging server"
            help="Specify the syslog server with hostname or IP address"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            yang_key="&LOGGING_REMOTE_HOST;"
            >
            <PARAM
                name="host"
                help="Hostname or IP address of the logging server"
                ptype="HOSTNAME_OR_IPADDR"
                yang_name="&LOGGING_REMOTE_HOST;=${host}"
                >
                <PARAM
                   name="severity"
                   help="severity level"
                   optional="true"
                   mode="subcommand"
                   ptype="SUBCOMMAND"
                   >
                   <PARAM
                     name="sub-command"
                     help=""
                     mode="switch"
                     ptype="SUBCOMMAND"
                     >
                   <PARAM
                      name="log-emerg"
                      help="System is unusable"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-emerg"
                     />
                   <PARAM
                      name="log-alert"
                      help="Immediate action needed"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-alert"
                     />
                     <PARAM
                      name="log-crit"
                      help="Critical conditions"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-crit"
                     />
                     <PARAM
                      name="log-err"
                      help="Error conditions"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-err"
                     />
                     <PARAM
                      name="log-warning"
                      help="Warning conditions"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-warning"
                     />
                     <PARAM
                      name="log-notice"
                      help="Normal but significant conditions (default severity)"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-notice"
                     />
                     <PARAM
                      name="log-info"
                      help="Informational messages"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-info"
                     />
                     <PARAM
                      name="log-debug"
                      help="Debug messages"
                      mode="subcommand"
                      ptype="SUBCOMMAND"
                      yang_name="&LOGGING_REMOTE_HOST_LEVEL;/remote-host-facility=local7,&LOGGING_REMOTE_HOST_LEVEL;/remote-host-level=log-debug"
                     />
                    </PARAM>
                </PARAM>
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>

        <!-- no logging server [string] -->
        <COMMAND
            name="no logging server"
            help="Delete a syslog server"
            operation="edit-config"
            namespace="&NAME_SPACE;"
            yang_name="&LOGGING_REMOTE;"
            >
            <PARAM
                 name="host"
                 help="Syslog server IP address or host name"
                 ptype="STRING"
                 sub_oper_yang="remove:logging-remote"
                 yang_name="&LOGGING_REMOTE_HOST;=${host}"
                >
            </PARAM>
            <ACTION
                builtin="clish_extn"
                />
        </COMMAND>

