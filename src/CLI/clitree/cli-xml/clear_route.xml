<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY SUBTREE_FILTER "Filter_Type:SUBTREE">
<!ENTITY NS_FILTER      "http://www.dellemc.com/networking/os10/dell-routing">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
			http://www.dellemc.com/force10/XMLSchema/clish.xsd">
<!-- ********************************************************************** -->
<!-- *  CLEAR IP ROUTE COMMANDS                                           * -->
<!--
clear ip route <ip-address|*>
clear ipv6 route <ipv6-address|*>
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->
    <VIEW name="enable-view">
        <COMMAND name="clear ip route"
            help="Clear routing information"
            ptype="SUBCOMMAND"
            operation="generic-rpc"
            namespace="&NS_FILTER;"
            yang_name="/clear-route/afn=ipV4"
            >
         <PARAM name="clear-ip-route-options"
            help=""
            ptype="SUBCOMMAND"
            mode="switch">
            <PARAM name="*"
                help="clear ip route *"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="/clear-route/clear-all=true">
            </PARAM>

        <PARAM name="prefix"
           help="Destination prefix"
           ptype="IP_ADDR_MASK"
           yang_name="/clear-route/address=${prefix}">
            </PARAM>
        </PARAM>
        <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
    <COMMAND name="clear ipv6 route"
            help="Clear routing information"
            ptype="SUBCOMMAND"
            operation="generic-rpc"
            namespace="&NS_FILTER;"
            yang_name="/clear-route/afn=ipV6"
            >
         <PARAM name="clear-ipv6-route-options"
            help=""
            ptype="SUBCOMMAND"
            mode="switch">
            <PARAM name="*"
                help="clear ipv6 route *"
                ptype="SUBCOMMAND"
                mode="subcommand"
                yang_name="/clear-route/clear-all=true">
            </PARAM>

        <PARAM name="prefix"
           help="Destination prefix"
           ptype="IPV6_ADDR_MASK"
           yang_name="/clear-route/address=${prefix}">
            </PARAM>
        </PARAM>
        <ACTION builtin="clish_extn"></ACTION>
        </COMMAND>
    </VIEW>
</CLISH_MODULE>
