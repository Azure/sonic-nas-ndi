<!DOCTYPE CLISH_MODULE [
]>

<CLISH_MODULE xmlns="http://www.dellemc.com/sonic/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/sonic/XMLSchema
                        http://www.dellemc.com/sonic/XMLSchema/clish.xsd">

   <VIEW
       name="configure-view"
       >
    <!-- interface commands -->
    <COMMAND
        name="ip prefix-list"
        help="Build a prefix list"
        >
        <PARAM
            name="prefix-name"
            help="Name of a prefix list"
            ptype="STRING"
            >
            <PARAM
                name="ip-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="permit"
                    help="Specify packets to forward"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Specify packets to reject"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION>
    python $SONIC_CLI_ROOT/sonic-cli-ip-prefix.py ip_prefix_create ${__full_line}
    </ACTION>
    </COMMAND>

    <COMMAND
        name="ipv6"
        help="ipv6 prefix-list">
    <ACTION>
        builtin="clish_nop"
    </ACTION>
    </COMMAND>

    <COMMAND
        name="ipv6 prefix-list"
        help="Build a prefix list"
        >
        <PARAM
            name="prefix-name"
            help="Name of a prefix list"
            ptype="STRING"
            >
            <PARAM
                name="ipv6-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="permit"
                    help="Specify packets to forward"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Specify packets to reject"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION> 
       python $SONIC_CLI_ROOT/sonic-cli-ip-prefix.py ip_prefix_create ${__full_line}
    </ACTION>
    </COMMAND>
    <COMMAND
        name="no ip prefix-list"
        help="Delete a prefix list"
        >
        <PARAM
            name="prefix-name"
            help="Name of a prefix list"
            ptype="STRING"
            >
            <PARAM
                name="ip-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                optional="true"
                >
                <PARAM
                    name="permit"
                    help="Specify packets to forward"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Specify packets to reject"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv4-prefix"
                        help="IP prefix network/length,  e.g.,  35.0.0.0/8"
                        ptype="IP_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_32"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION>
    python $SONIC_CLI_ROOT/sonic-cli-ip-prefix.py ip_prefix_delete ${__full_line}
    </ACTION>
    </COMMAND>

    <COMMAND
        name="no ipv6"
        help="Delete ipv6 prefix-list">
    <ACTION>
        builtin="clish_nop"
    </ACTION>
    </COMMAND>

    <COMMAND
        name="no ipv6 prefix-list"
        help="Delete ipv6 prefix list"
        >
        <PARAM
            name="prefix-name"
            help="Name of a prefix list"
            ptype="STRING"
            >
            <PARAM
                name="ipv6-prefix-subcommands"
                help="Prefix list sub commands"
                ptype="SUBCOMMAND"
                mode="switch"
                optional="true"
                >
                <PARAM
                    name="permit"
                    help="Specify packets to forward"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
		        <PARAM
		            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="RANGE_1_128"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
                <PARAM
                    name="deny"
                    help="Specify packets to reject"
                    mode="subcommand"
                    ptype="SUBCOMMAND"
                    >
                    <PARAM
                        name="ipv6-prefix"
                        help="IPv6 prefix network/length, e.g., 3ffe::/16"
                        ptype="IPV6_ADDR_MASK"
                        >
                        <PARAM
                            name="ge"
                            help="Minimum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="ge-min-prefix-length"
                                help="Minimum prefix length"
                                ptype="UINT"
                                >
                            </PARAM>
                        </PARAM>
                        <PARAM
                            name="le"
                            help="Maximum prefix length to be matched"
                            ptype="SUBCOMMAND"
                            mode="subcommand"
                            optional="true"
                            >
                            <PARAM
                                name="le-max-prefix-length"
                                help="Maximum prefix length"
                                ptype="UINT"
                                >
                            </PARAM>
                        </PARAM>
                    </PARAM>
                </PARAM>
            </PARAM>
        </PARAM>
    <ACTION>
    python $SONIC_CLI_ROOT/sonic-cli-ip-prefix.py ip_prefix_delete ${__full_line}
