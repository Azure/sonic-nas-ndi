<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY MODULE-NS-IETF "urn:ietf:params:xml:ns:yang:ietf-interfaces">
<!ENTITY MODULE-NS-IF "dell-if=http://www.dellemc.com/networking/os10/dell-interface">
<!ENTITY MODULE-NS-IETF-IF "&MODULE-NS-IETF;,&MODULE-NS-IF;">
<!ENTITY SUBTREE_FILTER                 "Filter_Type:SUBTREE">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->
    <VIEW name="enable-view">

    <!-- location-led system <node-id/unit-id> <on|off> -->
      <COMMAND
        name="location-led"
        help="Set location LED"
      >
      </COMMAND>
      <COMMAND
        name="location-led system"
        help="Set system location LED"
        ptype="SUBCOMMAND"
        operation="generic-rpc"
        namespace="http://www.dellemc.com/networking/os10/dell-equipment"
        >
        <PARAM
            name="system-id"
            help="Set system location LED with given system id"
            ptype="SYSTEM_ID"
            yang_name="/system-location-led/system-id=${system-id}"
            >
            <PARAM
                name="state"
                help="set location LED on or off"
                ptype="ON_OFF"
                yang_name="/system-location-led/led-on=${state}"
                />
        </PARAM>
        <ACTION
            builtin="clish_extn"
        ></ACTION>
      </COMMAND>

    <!-- location-led interface ethernet <node-id/unit-id/port-id> <on|off> -->
      <COMMAND
        name="location-led interface"
        help="Set interface location LED"
        ptype="SUBCOMMAND"
        operation="generic-rpc"
        namespace="&MODULE-NS-IETF-IF;"
        >
        <PARAM
            name="ifname"
            help="ethernet interface number"
            ptype="PHY_INTERFACE"
            yang_name="/dell-if:if-location-led/dell-if:ifname=${ifname}"
            >
            <PARAM
                name="state"
                help="Set location LED on or off"
                ptype="ON_OFF"
                yang_name="/dell-if:if-location-led/dell-if:led-on=${state}"
                />
        </PARAM>
        <ACTION
            builtin="clish_extn"
        ></ACTION>
      </COMMAND>

    </VIEW>
    <!--=======================================================-->
    <!--                Config MODE                            -->
    <!--=======================================================-->
</CLISH_MODULE>
