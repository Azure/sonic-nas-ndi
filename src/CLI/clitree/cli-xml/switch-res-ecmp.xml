<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
    <!ENTITY NAME_SPACE "http://www.dellemc.com/networking/os10/dell-switch-resource">
    <!ENTITY ECMP_GROUP "/ecmp-group">
    <!ENTITY MAX_PATH "&ECMP_GROUP;/max-path">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
            http://www.dellemc.com/force10/XMLSchema/clish.xsd"
    >
    <!--=======================================================-->

    <VIEW
        name="configure-view"
        >
        <COMMAND
            help="ECMP group configuration"
            name="ip ecmp-group"
            namespace="&NAME_SPACE;"
            operation="edit-config">
            <PARAM
               name="maximum-paths"
               help="Configure number of maximum-paths per ecmp-group"
               ptype="SUBCOMMAND"
               mode="subcommand"
               >
               <PARAM
                  name="value-maximum-paths"
                  help="Maximum ecmp-paths range 2-128 (default 64)"
                  ptype="RANGE_2_128"
                  yang_name="&MAX_PATH;=${value-maximum-paths}"
                  />
           </PARAM>
           <ACTION builtin="clish_extn"/>
       </COMMAND>
        <COMMAND
            help="ECMP Group Configuration"
            name="no ip ecmp-group"
            namespace="&NAME_SPACE;"
            operation="edit-config">
            <PARAM
               name="maximum-paths"
               help="Configure number of maximum-paths per ecmp-group"
               ptype="SUBCOMMAND"
               mode="subcommand"
               yang_name="&MAX_PATH;"
               >
               <PARAM
                  name="value-maximum-paths"
                  help="Maximum ecmp-paths range 2-128 (default 64)"
                  ptype="RANGE_2_128"
                  optional="true"
                  />
           </PARAM>
           <ACTION builtin="clish_extn"/>
       </COMMAND>
    </VIEW>
</CLISH_MODULE>
