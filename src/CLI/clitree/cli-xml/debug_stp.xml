<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY SUBTREE_FILTER "Filter_Type:SUBTREE">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd">
<!-- ********************************************************************** -->
<!-- *  DEBUG STP COMMANDS                                                * -->
<!--
debug spanning-tree all
debug spanning-tree bpdu tx
debug spanning-tree bpdu rx
debug spanning-tree events
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->
    <VIEW name="enable-view">

       <COMMAND name="debug spanning-tree"
                help="Debug command for xstp related information"
                ptype="SUBCOMMAND"
                operation="generic-rpc"
                namespace="http://www.dellemc.com/networking/os10/dell-xstp"
                pfeature="NOT_SUPPORTED"
                >
                <PARAM name="options1"
                       help=""
                       ptype="SUBCOMMAND"
                       mode="switch">
                       <PARAM name="all"
                              help="debug spanning-tree all"
                              ptype="SUBCOMMAND"
                              mode="subcommand"
                              yang_name="/debug-xstp/all"/>
                       <PARAM name="bpdu"
                              help="debug spanning-tree bpdu"
                              ptype="SUBCOMMAND"
                              mode="subcommand">
                              <PARAM name="options2"
                                     help=""
                                     ptype="SUBCOMMAND"
                                     mode="switch">
                                            <PARAM name="tx"
                                                   help="debug spanning-tree bpdu tx"
                                                   ptype="SUBCOMMAND"
                                                   mode="subcommand"
                                                   yang_name="/debug-xstp/bpdu-tx"/>
                                            <PARAM name="rx"
                                                   help="debug spanning-tree bpdu rx"
                                                   ptype="SUBCOMMAND"
                                                   mode="subcommand"
                                                   yang_name="/debug-xstp/bpdu-rx"/>
                              </PARAM>
                       </PARAM>
                       <PARAM name="events"
                              help="debug spanning-tree events"
                              ptype="SUBCOMMAND"
                              mode="subcommand"
                              yang_name="/debug-xstp/events"/>
                </PARAM>
        <ACTION builtin="clish_extn" > </ACTION>
        </COMMAND>
    </VIEW>
</CLISH_MODULE>
