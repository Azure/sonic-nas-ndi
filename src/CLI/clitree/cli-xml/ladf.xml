<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY MODULE-LADF "http://www.dellemc.com/networking/os10/dell-licensing">
<!ENTITY REMOTE_SYS                     "Install from remote file system">
<!ENTITY LOCAL_SYS                      "Install from local file system">
<!ENTITY REMOTE_SYS_REQ                 "Request to remote server">
<!ENTITY USB_SYS                        "Install from USB file system">
<!ENTITY FTP_URL                        "&REMOTE_SYS; (ftp://userid:passwd@hostip/filepath)">
<!ENTITY SCP_URL                        "&REMOTE_SYS; (scp://userid:passwd@hostip/filepath)">
<!ENTITY TFTP_URL                       "&REMOTE_SYS; (tftp://hostip/filepath)">
<!ENTITY SFTP_URL                       "&REMOTE_SYS; (sftp://userid:passwd@hostip/filepath)">
<!ENTITY HTTP_URL                       "&REMOTE_SYS; (http://hostip/filepath)">
<!ENTITY HTTP_POST_URL                  "&REMOTE_SYS_REQ; (http://hostip)">
<!ENTITY FILE_URL                       "&LOCAL_SYS; (localfs://filepath)">
<!ENTITY USB_URL                        "&USB_SYS;  (usb://filepath)">
]>
<CLISH_MODULE
        xmlns="http://www.dellemc.com/force10/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:xi="http://www.w3.org/2001/XInclude"
        xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
        http://www.dellemc.com/force10/XMLSchema/clish.xsd"
        >
        <!--=======================================================-->
        <!--                Exec MODE                              -->
        <!--=======================================================-->

        <VIEW
             name="enable-view"
             >
        <COMMAND
            name="show license"
            help="Show license and digital fulfillment related information"
            operation="get">
            <PARAM
                name="status"
                help="Show switch license and digital fulfillment status"
                ptype="SUBCOMMAND"
                mode="subcommand"
                template="show_ladf_status.xsl"
                operation="get"
                yang_filter="Filter_Type:SUBTREE;Filter_Conditions:/license-state;Filter_Namespace:&MODULE-LADF;"
                >
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>

      <!-- license install -->
      <COMMAND name="license"
        help="License and digital fulfillment commands"
        >
      </COMMAND>
        <COMMAND
            name="license install"
            help="Perform install operation"
            ptype="SUBCOMMAND"
            operation="generic-rpc"
            namespace="&MODULE-LADF;">
            <PARAM
                name="install_option"
                help="License install options"
                ptype="SUBCOMMAND"
                mode="switch"
                >
                <PARAM
                    name="ftp_install"
                    help="&FTP_URL;"
                    ptype="FTP_TYPE"
                    yang_name="/install-license/filename=${ftp_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
                <PARAM
                    name="http_install"
                    help="&HTTP_URL;"
                    ptype="HTTP_TYPE"
                    yang_name="/install-license/filename=${http_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
                <PARAM
                    name="http_post_install"
                    help="&HTTP_POST_URL;"
                    ptype="HTTP_TYPE"
                    yang_name="/install-license/filename=${http_post_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
                <PARAM
                name="localfs_install"
                    help="&FILE_URL;"
                    ptype="LOCAL_FS_TYPE"
                    yang_name="/install-license/filename=${localfs_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
                <PARAM
                    name="scp_install"
                    help="&SCP_URL;"
                    ptype="SCP_TYPE"
                    yang_name="/install-license/filename=${scp_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
                <PARAM
                    name="sftp_install"
                    help="&SFTP_URL;"
                    ptype="SFTP_TYPE"
                    yang_name="/install-license/filename=${sftp_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
                <PARAM
                    name="tftp_install"
                    help="&TFTP_URL;"
                    ptype="TFTP_TYPE"
                    yang_name="/install-license/filename=${tftp_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
                <PARAM
                    name="usb_install"
                    help="&USB_URL;"
                    ptype="USB_TYPE"
                    yang_name="/install-license/filename=${usb_install}"
                    template="ladf_install_status.xsl"
                    >
                </PARAM>
            </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
        </VIEW>

</CLISH_MODULE>
