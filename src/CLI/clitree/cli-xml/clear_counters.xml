<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE CLISH_MODULE [
<!ENTITY MODULE-NS-IETF "urn:ietf:params:xml:ns:yang:ietf-interfaces">
<!ENTITY MODULE-NS-IF "dell-if=http://www.dellemc.com/networking/os10/dell-interface">
<!ENTITY MODULE-NS-IETF-IF "&MODULE-NS-IETF;,&MODULE-NS-IF;">
]>
<CLISH_MODULE
    xmlns="http://www.dellemc.com/force10/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    xsi:schemaLocation="http://www.dellemc.com/force10/XMLSchema
                        http://www.dellemc.com/force10/XMLSchema/clish.xsd">
<!-- ********************************************************************** -->
<!-- *  CLEAR COUNTERS COMMANDS                                           * -->
<!--
clear counters [interface <interface>]
-->
<!-- *  View : enable-view                                                * -->
<!-- ********************************************************************** -->
    <VIEW name="enable-view">
        <COMMAND name="clear counters"
                 help="Clear counters"
                 ptype="SUBCOMMAND"
                 operation="generic-rpc"
                 namespace="&MODULE-NS-IETF-IF;"
                 prompt_fn="edit_and_confirm_clear_counters"
                 prompt_str="Clear all interface counters [confirm yes/no]:"
                 yang_name="/dell-if:clear-counters/dell-if:all-intf=ALL"
                 >
                 <PARAM name="interface"
                        help="Clear counters interface"
                        ptype="SUBCOMMAND"
                        mode="subcommand"
                        optional="true"
                        >
                        <PARAM
                            name="interface-switch"
                            help="interface type"
                            ptype="SUBCOMMAND"
                            mode="switch"
                            >
                            <PARAM
                                name="eee"
                                help="Clear EEE counters"
                                prompt_str="Clear all eee counters [confirm yes/no]:"
                                pfeature="FEATURE_EEE"
                                mode="subcommand"
                                ptype="SUBCOMMAND"
                                optional="true"
                                yang_name="/dell-if:clear-eee-counters/dell-if:all-intf=ALL">
                            </PARAM>
                            <PARAM name="ethernet"
                                   help="Clear ethernet interface counters"
                                   ptype="SUBCOMMAND"
                                   prompt_str="Clear all ethernet interface counters [confirm yes/no]:"
                                   mode="subcommand"
                                   yang_name="/dell-if:clear-counters/dell-if:all-intf=ALL-ETH"
                                   >
                                   <PARAM name="ifname"
                                          help="ethernet interface number"
                                          prompt_str="Clear counters on ethernet${ifname} [confirm yes/no]:"
                                          optional="true"
                                          ptype="IFACE_NUM"
                                          yang_name="/dell-if:clear-counters/dell-if:ifname=ethernet${ifname}">
                                          <PARAM
                                              name="eee"
                                              help="Energy efficient ethernet counters"
                                              prompt_str="Clear eee counters on ethernet${ifname} [confirm yes/no]:"
                                              pfeature="FEATURE_EEE"
                                              mode="subcommand"
                                              ptype="SUBCOMMAND"
                                              optional="true"
                                              yang_name="/dell-if:clear-eee-counters/dell-if:ifname=ethernet${ifname}"/>
                                   </PARAM>
                            </PARAM>
                            <PARAM name="fibre-channel"
                                   help="Clear fibre-channel interface counters"
                                   ptype="SUBCOMMAND"
                                   prompt_str="Clear all fibre-channel interface counters [confirm yes/no]:"
                                   pfeature="FEATURE_FC"
                                   mode="subcommand"
                                   yang_name="/dell-if:clear-counters/dell-if:all-intf=ALL-FC"
                                   >
                                   <PARAM name="ifname"
                                          help="fibre-channel interface number"
                                          prompt_str="Clear counters on fibre-channel${ifname} [confirm yes/no]:"
                                          optional="true"
                                          ptype="IFACE_NUM"
                                          yang_name="/dell-if:clear-counters/dell-if:ifname=fibre-channel${ifname}"/>
                            </PARAM>
                            <PARAM name="loopback"
                                   help="Clear loopback interface counters"
                                   ptype="SUBCOMMAND"
                                   prompt_str="Clear all loopback interface counters [confirm yes/no]:"
                                   mode="subcommand"
                                   yang_name="/dell-if:clear-counters/dell-if:all-intf=ALL-LOOPBACK"
                                   >
                                   <PARAM name="ifname"
                                          help="loopback interface number"
                                          prompt_str="Clear counters on loopback${ifname} [confirm yes/no]:"
                                          optional="true"
                                          ptype="LOOPBACK_NUM"
                                          yang_name="/dell-if:clear-counters/dell-if:ifname=loopback${ifname}"/>
                            </PARAM>
                            <PARAM name="management"
                                   help="Clear management interface counters"
                                   ptype="SUBCOMMAND"
                                   prompt_str="Clear all management interface counters [confirm yes/no]:"
                                   mode="subcommand"
                                   yang_name="/dell-if:clear-counters/dell-if:all-intf=ALL-MGMT"
                                   >
                                   <PARAM
                                          name="ifname"
                                          help="management interface number"
                                          prompt_str="Clear counters on mgmt${ifname} [confirm yes/no]:"
                                          optional="true"
                                          ptype="MGMT_INTF_NUM"
                                          yang_name="/dell-if:clear-counters/dell-if:ifname=mgmt${ifname}"/>
                            </PARAM>
                            <PARAM name="port-channel"
                                   help="Clear port-channel interface counters"
                                   ptype="SUBCOMMAND"
                                   prompt_str="Clear all port-channel interface counters [confirm yes/no]:"
                                   mode="subcommand"
                                   yang_name="/dell-if:clear-counters/dell-if:all-intf=ALL-LAG"
                                   >
                                   <PARAM name="ifname"
                                          help="port-channel interface number"
                                          prompt_str="Clear counters on port-channel${ifname} [confirm yes/no]:"
                                          optional="true"
                                          ptype="LAG_NUM"
                                          yang_name="/dell-if:clear-counters/dell-if:ifname=port-channel${ifname}"/>
                            </PARAM>
                            <PARAM name="vlan"
                                   help="Clear vlan interface counters"
                                   ptype="SUBCOMMAND"
                                   prompt_str="Clear all VLAN interface counters [confirm yes/no]:"
                                   mode="subcommand"
                                   yang_name="/dell-if:clear-counters/dell-if:all-intf=ALL-VLAN"
                                   >
                                   <PARAM name="ifname"
                                          help="Vlan number"
                                          prompt_str="Clear counters on vlan${ifname} [confirm yes/no]:"
                                          optional="true"
                                          ptype="VLAN_ID"
                                          yang_name="/dell-if:clear-counters/dell-if:ifname=vlan${ifname}"/>
                            </PARAM>
                        </PARAM>
                 </PARAM>
            <ACTION builtin="clish_extn"/>
        </COMMAND>
    </VIEW>
</CLISH_MODULE>

